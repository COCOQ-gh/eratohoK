;-------------------------------------------------
;セーブデータのアップデート処理
;-------------------------------------------------
@UPDATE
IF LASTLOAD_VERSION <= 161
	CONFIG:319 = 0
	FOR LOCAL, 0, 200
		IF EXISTCSV(LOCAL) && NO_TO_CHARA(LOCAL) != -1
			CSVTALENT LOCAL, 334
			TALENT:LOCAL:性的嗜好 = RESULT
		ENDIF
	NEXT
ENDIF

;易者、門番追加
IF LASTLOAD_VERSION <= 163
	FOR LOCAL, 0, 200
			IF NO_TO_CHARA(LOCAL) == -1 && EXISTCSV(LOCAL)
				ADDCHARA LOCAL
				CALL NEWCHARA_INIT(CHARANUM -1)
				CFLAG:(CHARANUM -1):12 = 1
			ENDIF
	NEXT
ENDIF