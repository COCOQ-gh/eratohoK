@INVESTIGATION
#DIM 対象
#DIMS 妖怪
#DIM 妖怪武闘値
#DIM 妖怪知略値
#DIM 与ダメージ
#DIM 被ダメージ

与ダメージ = 0
被ダメージ = 0


IF DVAR:異変解決度 > 0
	対象 = DVAR:異変解決者
	IF CFLAG:対象:捕虜先
		PRINTFORML 異変解決に向かっていた%ANAME(対象)%が捕虜になっているため異変解決は中断を余儀なくされた…
		DVAR:異変解決度 = 0
		DVAR:異変解決者 = -1
		DVAR:コンティニュー = 0
		RETURN -1
	ELSEIF CFLAG:対象:特殊状態 == 2
		PRINTFORML 異変解決に向かっていた%ANAME(対象)%が死亡してしまったため異変解決は中断を余儀なくされた…
		DVAR:異変解決度 = 0
		DVAR:異変解決者 = -1
		DVAR:コンティニュー = 0
		RETURN -1
	ELSEIF CFLAG:対象:所属 != CFLAG:MASTER:所属
		PRINTFORML 異変解決に向かっていた%ANAME(対象)%が他国に移ったため異変解決は中断を余儀なくされた…
		DVAR:異変解決度 = 0
		DVAR:異変解決者 = -1
		DVAR:コンティニュー = 0
		RETURN -1
	ENDIF
ENDIF

IF DVAR:異変解決度 == 0
	DVAR:異変解決者 = -1
	PRINTFORML 近頃妖怪たちが活性化しているとの報告を受けた
	PRINTFORML バラバラではなく集団で暴れているらしい
	PRINTFORMW もしかしたら妖怪たちが徒党を組んでいるのかもしれない
	PRINTFORML 戦争に忙しいが放置することもできないだろう
	PRINTFORMW どうしよう？
	CALL ASK_MULTI(@"武将に解決させる" ,"兵士に解決させる")
		IF RESULT == 1
			PRINTFORML やはり今は忙しい
			PRINTFORML 兵士たちに任せることにした
			$SOLDIER_MISSON
			PRINTFORML ・
			PRINTFORML ・
			PRINTFORMW ・
			IF RAND:4 != 0
				PRINTFORMW しかし兵士には荷が重かったようだ
				FOR LOCAL, 1, MAX_COUNTRY
					SIF IS_COUNTRY(LOCAL)
						CALL CHANGE_RELATION_C_TO_C(LOCAL, CFLAG:MASTER:所属, -10, 10)
				NEXT
				CALL COLORPRINT(@"被害が広がり軍の評判が下がった", カラー_注意)
				PRINTFORMW 
			ELSE
				PRINTFORML 兵士たちは見事任務を成し遂げた！
				FOR LOCAL, 1, MAX_COUNTRY
					SIF IS_COUNTRY(LOCAL)
						CALL CHANGE_RELATION_C_TO_C(LOCAL, CFLAG:MASTER:所属, 20, -20)
				NEXT
				CALL COLORPRINT(@"被害を食い止め軍の評判が上がった", カラー_注意)
				PRINTFORMW 
			ENDIF
			RETURN -1
		ELSE
			PRINTFORML 解決に向かうことにした
			PRINTFORMW 誰に任せよう？
			CALL SELECT_CHARA_LIST_SLG()
			対象 = RESULT
			IF RESULT < 0
				PRINTFORMW やはり兵士に任せることにした
				GOTO SOLDIER_MISSON
			ELSEIF 対象 == MASTER
				PRINTFORMW 自ら解決に出向くことにした
			ELSE
				PRINTFORMW %ANAME(対象)%に任せることにした
			ENDIF
			DVAR:異変解決者 = 対象
			DVAR:異変解決度 = 1
			DVAR:コンティニュー = 0
			DVAR:ボム = 3
			PRINTFORMW %ANAME(対象)%は早速異変解決に向かった
		ENDIF
ELSE
	対象 = DVAR:異変解決者
	PRINTFORMW %ANAME(対象)%は前回に引き続き異変解決に向かった
ENDIF

PRINTFORML ・
PRINTFORML ・
PRINTFORMW ・
LOCAL:0 = RAND:10 + DVAR:異変解決度
;餓鬼
IF LOCAL:0 <= 3
	妖怪 = 餓鬼
	PRINTFORML %妖怪%が現れた！
	PRINTFORMW 力も頭も弱そうだ
	妖怪武闘値 = 30
	妖怪知略値 = 30
;河童or天狗
ELSEIF LOCAL:0 <= 6
	IF RAND:2 == 0
		妖怪 = 河童
		PRINTFORML %妖怪%が現れた！
		PRINTFORMW 頭が良さそうだ
		妖怪武闘値 = 45
		妖怪知略値 = 60
	ELSE
		妖怪 = 天狗
		PRINTFORML %妖怪%が現れた！
		PRINTFORMW 力が強そうだ
		妖怪武闘値 = 60
		妖怪知略値 = 45
	ENDIF
;屍鬼or妖狐
ELSEIF LOCAL:0 <= 9
	IF ABL:対象:武闘 >= ABL:対象:知略
		妖怪 = 屍鬼
		PRINTFORML %妖怪%が現れた！
		PRINTFORMW 頭は悪そうだがとても力が強そうだ
		妖怪武闘値 = 75
		妖怪知略値 = 30
	ELSE
		妖怪 = 妖狐
		PRINTFORML %妖怪%が現れた！
		PRINTFORMW 力は弱そうだがとても頭が良さそうだ
		妖怪武闘値 = 30
		妖怪知略値 = 75
	ENDIF
;牛鬼
ELSE
	妖怪 = 牛鬼
	PRINTFORML %妖怪%が現れた！
	PRINTFORMW 強敵の様だ
	妖怪武闘値 = 75
	妖怪知略値 = 75
ENDIF
SIF IS_FEMALE(対象)
	PRINTFORMW 奴は%ANAME(対象)%を見て舌なめずりをしている
IF DVAR:異変解決度 == 6
	PRINTFORML どうやらこいつが今回の異変の首謀者の様だ
	PRINTFORMW %ANAME(対象)%は気を引き締めて勝負に挑んだ！
ENDIF
PRINTFORMW 戦闘開始！
$DANMAKU_BATTLE_LOOP
PRINTFORML どう動く？
CALL ASK_MULTI_JUDGE("正面から撃ち合う", 1,"カウンターを狙う", 1,@"ボム:残り{DVAR:ボム}発", DVAR:ボム > 0,"諦める", 1,"撤退", 1)
	IF RESULT == 2
		PRINTFORML ボムを使った！
		PRINTFORMW 敵にダメージを与えた
		与ダメージ ++
		DVAR:ボム -= 1
	ELSEIF RESULT == 4
		PRINTFORMW この敵は手強くて勝てなさそうだ、撤退することにした
		DVAR:コンティニュー ++
	ELSEIF RESULT == 3
		PRINTFORML 倒すことも逃げる事も出来そうにない
		PRINTFORMW %ANAME(対象)%は諦めてその場にへたり込んだ…
		被ダメージ += 3
	ELSEIF RESULT == 0
		PRINTFORMW 正面から撃ち合った！
		PRINTFORML 
		IF ABL:対象:武闘 * (RAND:8 + 1) > (妖怪武闘値 + DVAR:異変解決度) * (RAND:7 + 1)
			PRINTFORML 狙いが成功した！
			PRINTFORMW 敵にダメージを与えた
			与ダメージ ++
		ELSE
			PRINTFORML 見抜かれていた！
			PRINTFORMW ダメージを受けてしまった
			被ダメージ ++
		ENDIF
	ELSE
		PRINTFORMW カウンターを狙った！
		PRINTFORML 
		IF ABL:対象:知略 * (RAND:8 + 1) > (妖怪知略値 + DVAR:異変解決度) * (RAND:7 + 1)
			PRINTFORML 狙いが成功した！
			PRINTFORMW 敵にダメージを与えた
			与ダメージ ++
		ELSE
			PRINTFORML 見抜かれていた！
			PRINTFORMW ダメージを受けてしまった
			被ダメージ ++
		ENDIF
	ENDIF
IF 与ダメージ >= 3
	PRINTFORML 
	PRINTFORMW %妖怪%を撃退した！
	DVAR:異変解決度 ++
	IF DVAR:異変解決度 == 6
		PRINTFORML やった！
		PRINTFORMW ついに首領を撃破し、異変を解決した！
		PRINTFORML これでしばらくは領民たちにも被害は出ないだろう
		PRINTFORMW %ANAME(対象)%は多くの領民に感謝されながら帰路についた
		CALL PRINT_ADD_EXP(対象, "武闘経験値", RAND:50 + 21, 1)
		CALL PRINT_ADD_EXP(対象, "知略経験値", RAND:50 + 21, 1)
		IF 対象 == MASTER
			FOR LOCAL, 0, CHARANUM
			SIF !IS_ANIMAL(LOCAL)
				CFLAG:(LOCAL):好感度 += 100
			NEXT
			CALL COLORPRINT(@"異変解決の噂は各国に広まり%ANAME(対象)%の評価が上がった", カラー_注意)
		ENDIF
		PRINTFORMW 
	ELSE
		PRINTFORML しかしまだ他の妖怪が暴れているようだ
		CALL COLORPRINT(@"%ANAME(対象)%は次のステージへ進んだ", カラー_注意)
		PRINtFORMW 
		CALL PRINT_ADD_EXP(対象, "武闘経験値", RAND:10 + 1, 1)
		CALL PRINT_ADD_EXP(対象, "知略経験値", RAND:10 + 1, 1)
		PRINtFORMW 
	ENDIF
ELSEIF 被ダメージ >= 3
	PRINTFORML 
	PRINTFORMW 妖怪は手強くやられてしまった…
	IF IS_MALE(対象)
		PRINTFORMW %ANAME(対象)%は何とか逃げ帰った
	ELSE
		PRINTFORMW ボロボロになって横たわる%ANAME(対象)%に、%妖怪%がゆっくりと近づいてきた
		PRINTFORML 
		PRINTFORMW 敗北した%ANAME(対象)%が妖怪に凌辱されている…
		SELECTCASE RAND:32
			CASE 0
				PRINTFORML 彼は%ANAME(対象)%を四つん這いに這いつくばらせて激しくガン突きをしてくる
				PRINTFORML 一突き毎に膣肉を抉って来る荒々しいピストンに、%ANAME(対象)%は身体を震わせ呻き声を上げる
