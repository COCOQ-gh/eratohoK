@TAG_INIT(対象)
#DIM 対象
FOR LOCAL, 0, MAX_TAG
	TAG:対象:LOCAL = -1
NEXT
TRYCALLFORM INITIALIZE_TAG_{NO:対象}


@ADD_TAG(対象, タグ)
#DIM 対象
#DIM タグ
FOR LOCAL, 0, MAX_TAG
	IF TAG:対象:LOCAL == -1
		TAG:対象:LOCAL = タグ
		BREAK
	ENDIF
NEXT

@REMOVE_TAG(対象, タグ)
#DIM 対象
#DIM タグ
FOR LOCAL, 0, MAX_TAG
	IF TAG:対象:LOCAL == タグ
		TAG:対象:LOCAL = -1
		BREAK
	ENDIF
NEXT

@HAS_TAG(対象, タグ)
#FUNCTION
#DIM 対象
#DIM タグ
FOR LOCAL, 0, MAX_TAG
	SIF TAG:対象:LOCAL == タグ
		RETURNF 1
NEXT

RETURNF 0

@IS_TAG_EMPTY(対象)
#FUNCTION
#DIM 対象
FOR LOCAL, 0, MAX_TAG
	SIF TAG:対象:LOCAL != -1
		RETURNF 0
NEXT

RETURNF 1

