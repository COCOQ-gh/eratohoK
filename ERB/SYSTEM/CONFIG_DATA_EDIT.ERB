				FOR LOCAL:0, CFLAG:(ARG:0):1 + 1, MAX_COUNTRY
					IF IS_COUNTRY(LOCAL:0)
						CFLAG:(ARG:0):1 = LOCAL:0
						LOCAL:1 = 0
						BREAK
					ENDIF
				NEXT
				IF LOCAL:1
					CFLAG:(ARG:0):1 = 0
				ENDIF
			ENDIF
		ENDIF
	ELSEIF RESULT == 51
		LOCAL:1 = 0
		IF !F_COUNTRY_BOSS
			DRAWLINE
			LOCAL:1 ++
			PRINTFORML [0] 無所属
			LOCAL:1 ++
			FOR LOCAL, 0, MAX_COUNTRY
				SIF !IS_COUNTRY(LOCAL)
					CONTINUE
				PRINTFORML [{LOCAL}] %ANAME(GET_COUNTRY_BOSS(LOCAL))%
				LOCAL:1 ++
			NEXT
			INPUT
			LOCAL:1 ++
			IF RESULT == 0 || IS_COUNTRY(RESULT)
				CFLAG:(ARG:0):所属 = RESULT
				CFLAG:(ARG:0):12 = 0
				;所属がなければ放浪
				SIF RESULT == 0
					CFLAG:(ARG:0):12 = 1
			ENDIF
		ENDIF
		CLEARLINE LOCAL:1
	ELSEIF RESULT == 60
		LOCAL:1 = 1
		IF !F_COUNTRY_BOSS
			FOR LOCAL:0, CFLAG:(ARG:0):捕虜先 + 1, MAX_COUNTRY
				IF IS_COUNTRY(LOCAL:0)
					CFLAG:(ARG:0):捕虜先 = LOCAL:0
					;放浪フラグは折る
					SIF CFLAG:(ARG:0):特殊状態フラグ == 1
						CFLAG:(ARG:0):特殊状態フラグ = 0
					LOCAL:1 = 0
					BREAK
				ENDIF
			NEXT
		ENDIF
		IF LOCAL:1
			CFLAG:(ARG:0):捕虜先 = 0
			CFLAG:(ARG:0):29 = 0
			;所属がなければ放浪
			SIF !CFLAG:(ARG:0):所属
				CFLAG:(ARG:0):特殊状態フラグ = 1
		ENDIF
	ELSEIF RESULT == 61
		LOCAL:1 = 0
		IF !F_COUNTRY_BOSS
			DRAWLINE
			LOCAL:1 ++
			PRINTFORML [0] 捕虜でない
			LOCAL:1 ++
			FOR LOCAL, 0, MAX_COUNTRY
				SIF !IS_COUNTRY(LOCAL)
					CONTINUE
				PRINTFORML [{LOCAL}] %ANAME(GET_COUNTRY_BOSS(LOCAL))%
				LOCAL:1 ++
			NEXT
			INPUT
			LOCAL:1 ++
			IF RESULT == 0 || IS_COUNTRY(RESULT)
				CFLAG:(ARG:0):捕虜先 = RESULT
				CFLAG:(ARG:0):29 = 0
				;放浪キャラを捕虜にした場合はフラグを折る
				SIF CFLAG:(ARG:0):特殊状態フラグ == 1 && CFLAG:(ARG:0):捕虜先
					CFLAG:(ARG:0):特殊状態フラグ = 0
				;所属がないキャラが捕虜でなくなっていたら放浪させとく
				SIF CFLAG:(ARG:0):所属 && !CFLAG:(ARG:0):捕虜先
					CFLAG:(ARG:0):特殊状態フラグ = 1
			ENDIF
		ENDIF
		CLEARLINE LOCAL:1
	ELSEIF RESULT == 62 && CFLAG:(ARG:0):9 >= 1
		CFLAG:(ARG:0):29 = 0
	ELSEIF RESULT == 63 && CFLAG:(ARG:0):9 >= 1
		CFLAG:(ARG:0):29 = 1
	ELSEIF RESULT == 70
		PRINTL 好感度の値を入力して下さい(-1000～999999999999)
		INPUT
		CFLAG:(ARG:0):2 = LIMIT(RESULT, -1000, 999999999999)
		GOTO SHOW_LOOP1
	ELSEIF RESULT == 71
		PRINTL 依存度の値を入力して下さい(-1000～999999999999)
		INPUT
		CFLAG:(ARG:0):3 = LIMIT(RESULT, -1000, 999999999999)
		GOTO SHOW_LOOP1
	ELSEIF RESULT == 72
		PRINTL 従属度の値を入力して下さい(-1000～999999999999)
		INPUT
		CFLAG:(ARG:0):4 = LIMIT(RESULT, -1000, 999999999999)
		GOTO SHOW_LOOP1
	ELSEIF RESULT == 100 && ARG:0 != MASTER
		IF TALENT:(ARG:0):恋人
			TALENT:(ARG:0):恋人 = 0
		ELSE
			TALENT:(ARG:0):恋人 = 1
			TALENT:(ARG:0):烙印 = 0
		ENDIF
	ELSEIF RESULT == 101 && ARG:0 != MASTER
		IF TALENT:(ARG:0):恋慕
