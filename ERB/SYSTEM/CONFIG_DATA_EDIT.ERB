			PRINTFORM %LOCALS:1, 12, LEFT%
			PRINTFORM %LOCALS:2, 30, LEFT%
			PRINTFORM ★:{SUM_UNIT_STARS(LOCAL:0, LOCAL:1), 4, LEFT}
			PRINTFORM 兵:{UNIT_SOLDIER:(LOCAL:0):(LOCAL:1), 7, LEFT}
			PRINTFORM 攻:%TOSTR_UNIT_POWER_SHORT(UNIT_ATTACK_LEVEL(LOCAL:0, LOCAL:1)), 6, LEFT%
			PRINTFORM 防:%TOSTR_UNIT_POWER_SHORT(UNIT_DEFENSE_LEVEL(LOCAL:0, LOCAL:1)), 6, LEFT%
			PRINTFORM 知:%TOSTR_UNIT_POWER_SHORT(UNIT_INTELLIGENCE_LEVEL(LOCAL:0, LOCAL:1)), 6, LEFT%
			PRINTFORM 街:%GET_CITYNAME(UNIT_POSITION:(LOCAL:0):(LOCAL:1))%
			PRINTL
		NEXT
	NEXT
