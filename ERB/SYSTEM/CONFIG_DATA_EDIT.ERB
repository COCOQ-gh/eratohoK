			SELECTCASE CHECK_COUNTRY_RELATION_F(CFLAG:MASTER:1, LOCAL:0)
				CASE 0
					SETCOLOR COLOR("警告")
					PRINT 敵
				CASE 1
					SETCOLOR COLOR("停戦")
					PRINT 停
				CASE 2
					SETCOLOR COLOR("連合")
					PRINT 連
				CASE 3
					SETCOLOR COLOR("同盟")
					PRINT 同
				CASE 4
					SETCOLOR COLOR("永久同盟")
					PRINT 永
				CASE 5
					SETCOLOR COLOR("シアン")
					PRINT 自
			ENDSELECT
