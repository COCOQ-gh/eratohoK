			LOCALS:1 = 
			FOR LOCAL:1, 0, CHARANUM
				IF CFLAG:(LOCAL:1):1 == CFLAG:MASTER:1 && CFLAG:(LOCAL:1):9 == LOCAL:0
					IF LOCALS:1 == ""
						LOCALS:1 = %NAME:(LOCAL:1)%
					ELSE
