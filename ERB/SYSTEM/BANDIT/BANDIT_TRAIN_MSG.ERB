@TRAIN_BY_BANDIT()
#DIM 野盗番号
VARSET LOCAL
野盗番号 = GET_COUNTRY_FROM_ID(COUNTRY_BANDIT)
FOR LOCAL:0, 0, CHARANUM
	;野盗の捕虜であるか、「野盗に所属していて、捕虜でないか野盗の捕虜で、SPキャラでなく、1/5の確率または主人公」
	IF NO:LOCAL < 2000 && TRAIN_IS_SHOW_TEXT(LOCAL) && (!TALENT:LOCAL:野盗の肉便器 || (TALENT:LOCAL:野盗の肉便器 && !RAND:3))
		;マスターは条件を満たしても野盗の士官スタートなら抜ける
		SIF LOCAL == MASTER && BANDIT_START == 1
			CONTINUE
		IF !LOCAL:1
			DRAWLINE
			PRINTFORML 野盗に囚われた各国の仕官が陵辱されているようです……
			PRINTFORML
			LOCAL:1 = 1
		ENDIF
		CFLAG:(LOCAL:0):調教カウンタ ++
		;メッセ表示
		CALL BANDIT_TRAIN_MSG(LOCAL:0)
		WAIT
		CALL FORCE_FUCK(LOCAL:0, -16, GROUPMATCH(BANDIT_START, 1, 2))
		;あまり長く捕らえられていると寝返る
		IF (CFLAG:LOCAL:調教カウンタ >= TRAIN_FALLEN_TERM:3 && !TALENT:LOCAL:野盗の肉便器) || (CFLAG:LOCAL:所属 != 野盗番号 && TALENT:LOCAL:野盗の肉便器)
			SETCOLOR COLOR("警告")
			PRINTFORMW ……長きにわたる調教生活により、%ANAME(LOCAL)%の心は折れてしまったようだ
			PRINTFORMW %ANAME(LOCAL)%は野盗の性奴隷となり、寝返った
			RESETCOLOR
			CFLAG:LOCAL:所属 = 野盗番号
			CFLAG:LOCAL:捕虜先 = 0
			IF IS_LOVER(LOCAL) && CFLAG:MASTER:所属 != 野盗番号 && BANDIT_START != 1
				SETCOLOR COLOR("警告")
				PRINTFORML %ANAME(LOCAL)%の中で、%ANAME(MASTER)%への愛情よりも、野盗どもに使われる快楽が上回った
				RESETCOLOR
				CALL LOSE_RELATION_TALENT(LOCAL)
			ENDIF
			IF IS_SLAVE(LOCAL) && CFLAG:MASTER:所属 != 野盗番号 && BANDIT_START != 1
				SETCOLOR COLOR("警告")
				PRINTFORML %ANAME(LOCAL)%の中で、%ANAME(MASTER)%への忠誠よりも、野盗どもに使われる快楽が上回った
				RESETCOLOR
				CALL LOSE_RELATION_TALENT(LOCAL)
				TALENT:(LOCAL):烙印 = 0
			ENDIF
		ENDIF
	ENDIF
NEXT






