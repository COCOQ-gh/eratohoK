@TRAIN_BY_BANDIT()
#DIM 野盗番号
VARSET LOCAL
野盗番号 = GET_COUNTRY_FROM_ID(COUNTRY_BANDIT)
FOR LOCAL:0, 0, CHARANUM
	;野盗の捕虜であるか、「野党に所属していて、SPキャラでなく、1/5の確率または主人公」
	IF CFLAG:LOCAL:9 == 野盗番号 || (CFLAG:LOCAL:所属 == 野盗番号 && NO:LOCAL < 2000 && (RAND:100 < 20 || LOCAL == MASTER))
		IF !LOCAL:1
			DRAWLINE
			PRINTFORML 野盗に囚われた各国の仕官が陵辱されているようです……
			PRINTFORML
			LOCAL:1 = 1
		ENDIF
		;メッセ表示
		IF CONFIG:80
			CALL BANDIT_TRAIN_MSG(LOCAL:0)
		ENDIF
		CALL FORCE_FUCK(LOCAL:0, -16)
		;あまり長く捕らえられていると寝返る
		IF CFLAG:LOCAL:捕虜先 == 野盗番号 && CFLAG:LOCAL:解放カウンタ >= 10
			SETCOLOR COLOR("警告")
			PRINTFORMW ……長きにわたる調教生活により、%ANAME(LOCAL)%の心は折れてしまったようだ
			PRINTFORMW %ANAME(LOCAL)%は野盗の性奴隷となり、寝返った
			RESETCOLOR
			CFLAG:LOCAL:所属 = 野盗番号
			CFLAG:LOCAL:捕虜先 = 0
