FOR LOCAL:0, 0, CHARANUM
	IF CFLAG:LOCAL:9 == 野盗番号
		IF !LOCAL:1
			DRAWLINE
			PRINTFORML 野盗に囚われた各国の仕官が陵辱されているようです……
			PRINTFORML
			LOCAL:1 = 1
		ENDIF
		;好感度と従属度を失っていく
		CFLAG:(LOCAL:0):好感度 = MAX(CFLAG:(LOCAL:0):好感度 - MAX(CFLAG:(LOCAL:0):好感度 * 9 / 10, 200) , 0)
		CFLAG:(LOCAL:0):従属度 = MAX(CFLAG:(LOCAL:0):従属度 - MAX(CFLAG:(LOCAL:0):従属度 * 9 / 10, 200) , 0)
		;性処理と同等の経験を取得させる
		LOCAL:3 = 0
		LOCAL:3 += MAX(ABL:(LOCAL:0):Ｃ感 - 1, 0)
		LOCAL:3 += MAX(ABL:(LOCAL:0):Ｖ感 - 1, 0) * 3
		LOCAL:3 += MAX(ABL:(LOCAL:0):Ａ感 - 1, 0) * 3
		LOCAL:3 += MAX(ABL:(LOCAL:0):Ｂ感 - 1, 0)
		LOCAL:3 += MAX(ABL:(LOCAL:0):Ｍ感 - 1, 0) / 2
		LOCAL:3 = LOCAL:3 * (ABL:(LOCAL:0):欲望 + 2) / 2
			IF PENIS(LOCAL:0)
			EXP:(LOCAL:0):Ｃ感経験値 += ABL:(LOCAL:0):Ｃ感 + 1 + RAND:3
			EXP:(LOCAL:0):Ａ感経験値 += ABL:(LOCAL:0):Ａ感 * 30 + 20 + RAND:16
		ELSE
			EXP:(LOCAL:0):Ｃ感経験値 += ABL:(LOCAL:0):Ｃ感 * 5 + 5 + RAND:11
			EXP:(LOCAL:0):Ｖ感経験値 += ABL:(LOCAL:0):Ｖ感 * 20 + 15 + RAND:11
			EXP:(LOCAL:0):Ａ感経験値 += ABL:(LOCAL:0):Ａ感 * 20 + 15 + RAND:11
			ENDIF
		EXP:(LOCAL:0):Ｂ感経験値 += ABL:(LOCAL:0):Ｂ感 * 4 + 4 + RAND:9
		EXP:(LOCAL:0):Ｍ感経験値 += ABL:(LOCAL:0):Ｍ感 * 4 + 4 + RAND:9
		EXP:(LOCAL:0):欲望経験値 += ABL:(LOCAL:0):欲望 * 3 + 2 + LOCAL:3 + RAND:7
		EXP:(LOCAL:0):奉仕経験値 += ABL:(LOCAL:0):奉仕 * 2 + 5 + RAND:11
		EXP:(LOCAL:0):性交経験値 += ABL:(LOCAL:0):性交 * 5 + 15 + RAND:11
		EXP:(LOCAL:0):性技経験値 += ABL:(LOCAL:0):性技 + 2 + RAND:4
		IF IS_MALE(LOCAL:0)
			EXP:(LOCAL:0):ＢＬ経験値 += 25 + RAND:11
		ENDIF
		EXP:(LOCAL:0):精愛経験値 += ABL:(LOCAL:0):精愛 * 10 + 30 + RAND:21
		IF PENIS(LOCAL:0)
			EXP:(LOCAL:0):射精経験値 += ABL:(LOCAL:0):射精 * 10 + 30 + RAND:21
		ENDIF
		EXP:(LOCAL:0):キス経験 += 2 + RAND:8
		EXP:(LOCAL:0):口淫経験 += 3 + RAND:10
		EXP:(LOCAL:0):絶頂経験 += LOCAL:3 * (60 + RAND:81) / 100
		;中出しと妊娠判定
		IF CONFIG:1 == 0 && (VAGINA(LOCAL:0) || TALENT:(LOCAL:0):孕み可能)
			FOR LOCAL:1, 440, 480
				TCVAR:(LOCAL:0):(LOCAL:1) = 0
			NEXT
			CALL RECORD_INJECTION(LOCAL:0, -16, 3)
			CALL CHECK_PREGNANT(LOCAL:0)
		ENDIF
		;メッセ表示
		IF CONFIG:80
			CALL BANDIT_TRAIN_MSG(LOCAL:0)
		ENDIF
		;能力上昇メッセ
		CALL TRAIN_AUTO_ABLUP(LOCAL:0, 0)
		;処女とキス未経験を失う
		IF TALENT:(LOCAL:0):キス未経験
			IF RAND:4
				;ペニスへのキス
				CALL KISS_COMMON(LOCAL:0, -16, 1, 0, 0)
			ENDIF
			;唇へのキス
			CALL KISS_COMMON(LOCAL:0, -16, 0, 0, 0)
			SETCOLOR COLOR_CAUTION
			PRINTFORML %ANAME(LOCAL:0)%は<キス未経験>を失った…
			RESETCOLOR
		ENDIF
		IF TALENT:(LOCAL:0):処女 && VAGINA(LOCAL:0)
			SETCOLOR COLOR_CAUTION
			PRINTFORML %ANAME(LOCAL:0)%は<処女>を失った…
			RESETCOLOR
			;処女を捧げた相手を野盗に設定
			CFLAG:(LOCAL:0):41 = -16
			TALENT:(LOCAL:0):処女 = 0
		ENDIF
	ENDIF
NEXT




IF LOCAL:1
	DRAWLINE
ENDIF

