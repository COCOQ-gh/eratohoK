@EVENT_OTHERS_RAN_PRISONER
#DIM TARGET_LIST, 100
#DIM TARGET_NUM

;コンフィグがOFFなら戻る
IF CONFIG:81 == 0
	RETURN
ENDIF

LOCAL:8 = 0

;タチ役をCONFIG:88で設定
LOCAL:5 = CONFIG:88
LOCAL:4 = GET_COUNTRY_FROM_BOSS_ID(GET_ID(LOCAL:5))

;タチ役が不在or主人公or捕虜 または タチ役が君主でない なら戻る
IF LOCAL:5 < 0 || LOCAL:5 == MASTER || CFLAG:(LOCAL:5):9 != 0 || LOCAL:4 <= 0
	RETURN
ENDIF

;現在調教中の捕虜を取得
LOCAL:6 = ID_TO_CHARA(CFLAG:(LOCAL:5):45)
IF LOCAL:6 >= 0
	;調教条件を満たしていなければ解除
	IF CONFIG:81 == 1 && IS_MALE(LOCAL:6)
		LOCAL:6 = -1
	ENDIF
	IF CFLAG:(LOCAL:6):9 != LOCAL:4
		LOCAL:6 = -1
	ENDIF
ENDIF

;調教中の捕虜がいない場合は新たに設定
IF LOCAL:6 < 0
	TARGET_NUM = 0
	FOR LOCAL:0, 0, CHARANUM
		;タチ役勢力の捕虜を調べる
		IF LOCAL:0 != MASTER && CFLAG:(LOCAL:0):9 == LOCAL:4 && TARGET_NUM < 100 && !(CONFIG:81 == 1 && IS_MALE(LOCAL:0))
			TARGET_LIST:TARGET_NUM = LOCAL:0
			TARGET_NUM ++
		ENDIF
	NEXT
	IF TARGET_NUM >= 1
		;該当する捕虜の中からランダムで調教対象を選択
