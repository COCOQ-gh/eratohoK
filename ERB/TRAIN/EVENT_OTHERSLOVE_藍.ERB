;-------------------------------------------------
;キャラARG:0がタチ役と懇意になる条件を満たしているかどうかを判定する関数
;ARG:1=タチ役のキャラ番号
;-------------------------------------------------
@IS_LOVE_FOR_RAN(ARG:0, ARG:1)
#FUNCTION
;<除外> 主人公、タチ役本人、霖之助、男性、幼児
IF ARG:0 != MASTER && ARG:0 != ARG:1 && CFLAG:(ARG:0):1 == CFLAG:(ARG:1):1 && IS_FEMALE(ARG:0) && !TALENT:(ARG:0):幼児
	;<除外> タチ役の子供、親愛、隷属、烙印
	IF CFLAG:(ARG:0):24 != GET_ID(ARG:1) && !TALENT:(ARG:0):親愛 && !TALENT:(ARG:0):隷属 && !TALENT:(ARG:0):烙印
		;タチ役になびくオプションがONなら恋慕・恋人・服従も対象に
		IF CONFIG:72 == 1 || (!TALENT:(ARG:0):恋慕 && !TALENT:(ARG:0):恋人 && !TALENT:(ARG:0):服従)
			;タチ役と同一勢力で捕虜でない
			IF CFLAG:(ARG:0):1 == CFLAG:(ARG:1):1 && CFLAG:(ARG:0):9 == 0
				;霖之助・タチ役が手をつける範囲の設定  0=無制限、1=主人公と面識、2=恋慕・服従
				IF CONFIG:83 == 0
					RETURNF 1
				ELSEIF CONFIG:83 == 1 && CFLAG:(ARG:0):17 == 1
					RETURNF 1
				ELSEIF CONFIG:82 == 2 && (TALENT:(ARG:0):恋慕 || TALENT:(ARG:0):服従 || TALENT:(ARG:0):藍に恋慕 || TALENT:(ARG:0):藍の恋人)
					RETURNF 1
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURNF 0


;-------------------------------------------------
;キャラARG:0が外道タチ役に摘み食いされる条件を満たしているかどうかを判定する関数
;ARG:1=タチ役のキャラ番号
;-------------------------------------------------
@IS_LOVE_FOR_RAN_B(ARG:0, ARG:1)
#FUNCTION
;<除外> 主人公、タチ役本人、霖之助、男性、幼児
IF ARG:0 != MASTER && ARG:0 != ARG:1 && CFLAG:(ARG:0):1 == CFLAG:(ARG:1):1 && IS_FEMALE(ARG:0) && !TALENT:(ARG:0):幼児
	LOCAL:2 = 0
	;条件1:タチ役と同一勢力で捕虜でない
	IF CFLAG:(ARG:0):1 == CFLAG:(ARG:1):1 && CFLAG:(ARG:0):9 == 0
		LOCAL:2 = 1
	;条件2:タチ役勢力の捕虜
	ELSEIF CFLAG:(ARG:0):9 == CFLAG:(ARG:1):1
		LOCAL:2 = 1
	ENDIF
	;どちらかの条件を満たした場合
	IF LOCAL:2
		;暴君タチ役がつまみ食いする範囲の設定  0=無制限、1=主人公と面識、2=恋慕・服従
		IF CONFIG:84 == 0
			RETURNF 1
		ELSEIF CONFIG:84 == 1 && CFLAG:(ARG:0):17 == 1
			RETURNF 1
		ELSEIF CONFIG:84 == 2 && (TALENT:(ARG:0):恋慕 || TALENT:(ARG:0):服従 || TALENT:(ARG:0):藍に恋慕 || TALENT:(ARG:0):藍の恋人)
			RETURNF 1
		ENDIF
	ENDIF
ENDIF
RETURNF 0



