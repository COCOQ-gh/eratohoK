@COM_NAME292
RESULTS:0 = なにもしない

;-------------------------------------------------
;選択可否判定
;-------------------------------------------------
@COM_ABLE292
;共通部分
CALL COM_ABLE_COMMON(299)
SIF RESULT == 0
	RETURN 0
;プレイヤーは最大で1人まで
SIF MPLY_NUM <= 0 || MPLY_NUM > 1
	RETURN 0
;プレイヤーが行動不能なら不可(ターゲットは体力切れなら可)
SIF !IS_PLAYABLE(MPLY:0)
	RETURN 0
;主人公がプレイヤーのとき限定
SIF MPLY:0 != MASTER
	RETURN 0
;主人公に主導権があるとき限定
SIF IS_INITIATIVE(MASTER) == 0
	RETURN 0
RETURN 1

