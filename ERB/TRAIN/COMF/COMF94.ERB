	CASE 2
		TIMES LOCAL:10, 0.75
	CASE 3
		TIMES LOCAL:10, 0.60
ENDSELECT

SELECTCASE MEQUIP_TARGET_NUM:(ARG:0)
	CASE 2
		TIMES LOCAL:10, 0.75
ENDSELECT

;●全プレイヤーについて判定
FOR LOCAL:0, 0, MEQUIP_PLAYER_NUM:(ARG:0)
	LOCAL:2 = MEQUIP_PLAYER:(ARG:0):(LOCAL:0)

	DOWNBASE:(LOCAL:2):体力 += 20

	EXP:(LOCAL:2):性技経験値 += 1
	EXP:(LOCAL:2):口淫経験 += 1

	SOURCE:(LOCAL:2):奉仕 += SERVE_HOUSHI(LOCAL:2, 150)
	SOURCE:(LOCAL:2):接触 += 20
	SOURCE:(LOCAL:2):快Ｍ += 100 * COM94_RATE_M(LOCAL:2) / 1000
	SOURCE:(LOCAL:2):性行動 += 100
	SOURCE:(MTAR:0):不潔 = 100
	SOURCE:(MTAR:0):逸脱 = 100
	SOURCE:(MTAR:0):苦痛 = 50
	;奉仕経験値を得られるコマンドのフラグ
	TCVAR:(LOCAL:2):4 = 1
	;豚の射精ゲージを追加
	TFLAG:102 += SENSE_HOUSHI_P(LOCAL:2, MTAR:0, 1500) * LOCAL:10 / 100
	IF TFLAG:102 >= 10000
		PRINTFORML 豚は%ANAME(LOCAL:2)%の口内に射精した……
		EXP:(LOCAL:2):精愛経験値 += 1
		TFLAG:102 -= 10000
	ENDIF
NEXT



