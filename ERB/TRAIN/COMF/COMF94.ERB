	CALL RELEASE_EQUIP(SEARCH_EQUIP(2, MPLY:(LOCAL:0), -1), 1, 0)
NEXT

;対Ｐ奉仕系コマンドの継続フラグを解除
CALL RELEASE_SERVE_P

;●人数補正の設定
LOCAL:10 = 100

SELECTCASE MPLY_NUM
	CASE 2
		TIMES LOCAL:10, 0.75
	CASE 3
		TIMES LOCAL:10, 0.60
ENDSELECT

;●全プレイヤーについて判定
FOR LOCAL:0, 0, MPLY_NUM
	LOCAL:2 = MPLY:(LOCAL:0)

	DOWNBASE:(LOCAL:2):体力 += 120

	EXP:(LOCAL:2):性技経験値 += 1
	EXP:(LOCAL:2):口淫経験 += 1

	SOURCE:(LOCAL:2):奉仕 = SERVE_HOUSHI(LOCAL:2, 400)
	SOURCE:(LOCAL:2):接触 = 50
	SOURCE:(LOCAL:2):快Ｍ = 300 * COM94_RATE_M(LOCAL:2) / 1000
	SOURCE:(LOCAL:2):性行動 = 300
	SOURCE:(MTAR:0):不潔 = 250
	SOURCE:(MTAR:0):逸脱 = 250
	SOURCE:(MTAR:0):苦痛 = 200
	
	;主導権に応じた優越・受動のソース追加
	CALL ADD_SOURCE_INITIATIVE_U(LOCAL:2, 150, 120)

	;奉仕経験値を得られるコマンドのフラグ
	TCVAR:(LOCAL:2):4 = 1


	;ペニスへのキス
	CALL KISS_COMMON(LOCAL:2, -14, 1, 0, 1)


	;豚の射精ゲージを追加
	TFLAG:102 += SENSE_HOUSHI_P(LOCAL:2, MTAR:0, 1500) * LOCAL:10 / 100
	IF TFLAG:102 >= 10000
		PRINTFORML 豚は%ANAME(LOCAL:2)%の口内に射精した……
		EXP:(LOCAL:2):精愛経験値 += 1
		TFLAG:102 -= 10000
	ENDIF
	;ペニスへのキス
	CALL KISS_COMMON(LOCAL:2, -14, 1, 0, 1)
	;豚の射精ゲージを追加
	TFLAG:102 += SENSE_HOUSHI_P(LOCAL:2, MTAR:0, 1500) * LOCAL:10 / 100
	IF TFLAG:102 >= 10000
		PRINTFORML 豚は%ANAME(LOCAL:2)%の口内に射精した……
		TFLAG:102 -= 10000
	ENDIF
NEXT

;主導度変化基準値
TFLAG:49 = 2

;倒錯度変化基準値
TFLAG:50 = -3

;レズ・ＢＬ経験基準値
TFLAG:51 = 7

RETURN 1

