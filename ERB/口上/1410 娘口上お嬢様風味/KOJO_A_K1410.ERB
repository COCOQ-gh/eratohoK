;=================================================
;●「会いに行く」の開始時のセリフ
;=================================================
@KOJO_TRAIN_START_A1_K1410
;[虚ろ]状態なら口上を表示しない
IF TALENT:虚ろ
	RETURN
ENDIF

;-------------------------------------------------
;初回
;-------------------------------------------------
IF CFLAG:200 == 0
	CFLAG:200 = 1
	;初対面の場合
	IF !CFLAG:17
		PRINTFORML 「……貴方がわたくしのおとうさまですか？」
		PRINTFORMW 「これからよろしくお願いいたしますわ」
	;既に会ったことがある場合
	ELSE
		PRINTFORMW 「おとうさま、改めまして…これからよろしくお願いしますわ」
	ENDIF

;-------------------------------------------------
;開始時(1回のみ)
;-------------------------------------------------
;既成事実Lv3(初めてセックスをした次の回)
ELSEIF CFLAG:200 < 5 && MARK:2 == 3
	CFLAG:200 = 5
	PRINTFORML 「わたくしをお抱きになったその意味」
	PRINTFORMW 「よぉく、よぉく考えてくださいましね？おとうさま」
;既成事実Lv2(恋人になった次の回)
ELSEIF CFLAG:200 < 4 && MARK:2 == 2
	CFLAG:200 = 4
	PRINTFORMW 「……これからも末永くお願いしますわ、おとうさま」
;既成事実Lv1(初めてキスをした次の回)
ELSEIF CFLAG:200 < 3 && MARK:2 == 1
	CFLAG:200 = 3
	PRINTFORML 「………なっ、なんでもありませんわっ」
	PRINTFORMW %CALLNAME%は紅くなった顔を逸らした
;既成事実Lv0で恋慕
ELSEIF CFLAG:200 < 2 && MARK:2 == 0 && TALENT:恋慕
	CFLAG:200 = 2
	PRINTFORML 「おとうさま･･･おとうさま」
	PRINTFORMW %CALLNAME%は『おとうさま』を連発しながら%CALLNAME:MASTER%の背中に抱きついている

;-------------------------------------------------
;開始時(2回目以降)
;-------------------------------------------------
;既成事実Lv3かつ恋慕
ELSEIF MARK:2 == 3 && TALENT:恋慕
	PRINTFORML 「おとうさま……わたくし今とても幸せですわ」
	PRINTFORMW そんなことを呟きながら%CALLNAME%は%CALLNAME:MASTER%の背中に抱きついている
;既成事実Lv3(セックス済み)
ELSEIF MARK:2 == 3
	PRINTFORMW 「…おとうさまはまた、わたくしをお抱きになりたいのですか?」
;既成事実Lv2(恋人)
ELSEIF MARK:2 == 2
	PRINTFORML 「あら、おとうさま。今日はこちらにいらしていたのですね」
	PRINTFORMW そう言った%CALLNAME%はすっと近寄って%CALLNAME:MASTER%の腕に抱きついてきた
;既成事実Lv1(キス済み)
ELSEIF MARK:2 == 1
	PRINTFORMW 「おとうさま、来るなら来ると言っておいてくださいな」
;既成事実Lv0で恋慕
ELSEIF MARK:2 == 0 && TALENT:恋慕
	PRINTFORML 「おとうさま」
	PRINTFORML %CALLNAME:MASTER%は%CALLNAME%の方に顔を向けた
	PRINTFORMW 「いえ、呼んでみただけですわ。そう、ただそれだけ……ですわ」
;既成事実Lv0
ELSEIF MARK:2 == 0
	PRINTFORMW 「おとうさま、今日のご用事はなんでしょう?」
ENDIF

;=================================================
;●「会いに行く」の終了時のセリフ
;=================================================
@KOJO_TRAIN_END_A1_K1410
;[虚ろ]状態なら口上を表示しない
IF TALENT:虚ろ
	RETURN
ENDIF

;-------------------------------------------------
;行動不能の場合
;-------------------------------------------------
;酒酔いによる行動不能
IF TCVAR:53 == 1
	PRINTFORML 「おひょ…ぅしゃ･･･ま♪」
	PRINTFORMW %CALLNAME:MASTER%の背中に抱きついたまま、%CALLNAME%は酔いつぶれて寝てしまった
;快感のあまり気絶
ELSEIF TCVAR:52
	PRINTFORML 「もぅ…りゃめりぇ、しゅの…♪おひょぅ…しゃまの、ひぇんしゃい…♪」
	PRINTFORMW 限界を超えて%CALLNAME:MASTER%に愛された%CALLNAME%は、いまだ動けぬ肢体を震わせている
;疲労による行動不能
ELSEIF TCVAR:51
	PRINTFORML 「はぁ…うぅ、もぅ…限界ですわぁぁ……」
	PRINTFORMW そう言って倒れこんだ%CALLNAME%は、すぐに穏やかな寝息を立て始めた……
ENDIF

;-------------------------------------------------
;終了時(1回のみ)
;-------------------------------------------------
;既成事実Lv3(初めてセックスをした)
IF CFLAG:201 < 5 && MARK:2 == 3
	CFLAG:201 = 5
	;行動不能ならフラグだけ立てて表示はしない
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		PRINTFORMW 「とてもとても、背徳的な時間でしたわ……ねぇ？おとうさま」
	ENDIF
;既成事実Lv2(恋人になった)
ELSEIF CFLAG:201 < 4 && MARK:2 == 2
	CFLAG:201 = 4
	;行動不能ならフラグだけ立てて表示はしない
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		PRINTFORML 「おとうさまとの関係は変わりましたけれど……」
		PRINTFORMW 「おとうさまとの日常はどう変わるのか、楽しみですわぁ♪」
	ENDIF
;既成事実Lv1(初めてキスをした)
ELSEIF CFLAG:201 < 3 && MARK:2 == 1
	CFLAG:201 = 3
	;行動不能ならフラグだけ立てて表示はしない
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		PRINTFORML 「娘の唇を奪うという行為」
		PRINTFORML 「とてもとても、よぉく考えてくださいましね、おとうさま」
		PRINTFORMW %CALLNAME%の様子がいつもと少し違う様に思えたが……気にしないことにした
	ENDIF

;-------------------------------------------------
;終了時(2回目以降)　
;-------------------------------------------------
;行動不能なら非表示
ELSEIF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
	;既成事実Lv3かつ恋慕
	IF MARK:2 == 3 && TALENT:恋慕
		PRINTFORMW 「おとうさま、ごきげんよう。明日もまたお待ちしていますわ」
	;既成事実Lv3(セックス済み)
	ELSEIF MARK:2 == 3
		PRINTFORMW 「明日も…お越しになりますの？」
	;既成事実Lv2(恋人)
	ELSEIF MARK:2 == 2
		PRINTFORMW 「結局何時も通りの日常でしたわ…　では明日もお待ちしていますわね、おとうさま」
	;既成事実Lv1(キス済み)
	ELSEIF MARK:2 == 1
		PRINTFORMW 「あら、お帰りですかおとうさま。それではまた、ごきげんよう」
	;既成事実Lv0で恋慕
	ELSEIF MARK:2 == 0 && TALENT:恋慕
		PRINTFORMW 「これ以上は……だめですわ」
	;既成事実Lv0
	ELSEIF MARK:2 == 0
		PRINTFORMW 「それでは、ごきげんよう。おとうさま」
	ENDIF
ENDIF

