;-------------------------------------------------
;主人公を射精させた
;-------------------------------------------------
IF NOWEX:MASTER:射精 > 0
	SELECTCASE TCVAR:MASTER:420
		;膣に射精
		CASE 1
			;恋慕 or 服従
			IF TALENT:恋慕 || TALENT:服従
				;PRINTFORMW 
			;それ以外
			ELSE
				;PRINTFORMW 
			ENDIF
		;アナルに射精
		CASE 2
			;恋慕 or 服従
			IF TALENT:恋慕 || TALENT:服従
				;PRINTFORMW 
			ELSE
				;PRINTFORMW 
			ENDIF
		;手に射精
		CASE 3
			;恋慕 or 服従
			IF TALENT:恋慕 || TALENT:服従
				;PRINTFORMW 
			;それ以外
			ELSE
				;PRINTFORMW 
			ENDIF
		;口に射精
		CASE 4
			;恋慕 or 服従 かつ精愛Lv2以上
			IF (TALENT:恋慕 || TALENT:服従) && ABL:精愛 >= 2
				;PRINTFORMW 
			;恋慕 or 服従
			ELSEIF TALENT:恋慕 || TALENT:服従
				;PRINTFORMW 
			;それ以外
			ELSE
				;PRINTFORMW 
			ENDIF
		;胸に射精
		CASE 5
			;恋慕 or 服従
			IF TALENT:恋慕 || TALENT:服従
				;PRINTFORMW 
			;それ以外
			ELSE
				;PRINTFORMW 
			ENDIF
		;足に射精
		CASE 6
		;尻尾に射精
		CASE 7
		;顔に射精
		CASE 8
		;髪に射精
		CASE 9
		;腋に射精
		CASE 10
		;その他の場所に射精
		CASEELSE
	ENDSELECT

;-------------------------------------------------
;主人公を射精させずに絶頂させた
