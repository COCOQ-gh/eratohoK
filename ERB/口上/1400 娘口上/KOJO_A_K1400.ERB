;=================================================
;●「会いに行く」の開始時のセリフ
;=================================================
@KOJO_TRAIN_START_A1_K1400
;[虚ろ]状態なら口上を表示しない
IF TALENT:虚ろ
	RETURN
ENDIF

;-------------------------------------------------
;初回
;-------------------------------------------------
IF CFLAG:200 == 0
	CFLAG:200 = 1
	;初対面の場合
	IF !CFLAG:17
		PRINTFORML 「えっと、初めまして。貴方がボクのとーさまなの？」
		PRINTFORMW 「ふうん。まあ、よろしくね」
			;娘の武器を決定する
			SELECTCASE RAND:6
			CASE 0
				;直剣を使う
				CFLAG:1583 = 1
			CASE 1
				;槍を使う
				CFLAG:1583 = 2
			CASE 2
				;鉄球を使う
				CFLAG:1583 = 3
			CASE 3
				;戟を使う
				CFLAG:1583 = 4
			CASE 4
				;連接剣を使う
				CFLAG:1583 = 5
			CASE 5
				;チェーンソーを使う
				CFLAG:1583 = 6
			ENDSELECT
			RETURN 0
	;既に会ったことがある場合
	ELSE
		;PRINTFORMW 
	ENDIF

;-------------------------------------------------
;開始時(1回のみ)
;-------------------------------------------------
;既成事実Lv3(初めてセックスをした次の回)
ELSEIF CFLAG:200 < 5 && MARK:2 == 3
	CFLAG:200 = 5
	PRINTFORM 「父娘でそーいう関係ってなんだか”はいとくてき”だね！」
	PRINTFORMW 「え、背徳的の意味？　……だいじょーぶ、わかんなくたって生きていけるよ！」
;既成事実Lv2(恋人になった次の回)
ELSEIF CFLAG:200 < 4 && MARK:2 == 2
	CFLAG:200 = 4
	PRINTFORMW 「えへへ……なんだか恥ずかしいね」
;既成事実Lv1(初めてキスをした次の回)
ELSEIF CFLAG:200 < 3 && MARK:2 == 1
	CFLAG:200 = 3
	PRINTFORM 「とーさま、その……またキス、しようね……」
	PRINTFORMW %CALLNAME%は真っ赤な顔でそう言って、俯いてしまった。
;既成事実Lv0で恋慕
ELSEIF CFLAG:200 < 2 && MARK:2 == 0 && TALENT:恋慕
	CFLAG:200 = 2
	PRINTFORM 「とーさま！　またボクに会いに来てくれたんだ」
	PRINTFORM 「うん、すっごく嬉しいよ。で、今日は何する？　何する？」
	PRINTFORMW そう言って%CALLNAME%は跳ね回らんばかりの様子で %CALLNAME:MASTER%を見つめている。

;-------------------------------------------------
;開始時(2回目以降)
;-------------------------------------------------
;既成事実Lv3かつ恋慕
ELSEIF MARK:2 == 3 && TALENT:恋慕
	SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「とーさま♪　……ううん、呼んでみただけ」
			PRINTFORMW 「嫌だった？」
			PRINTFORMW %CALLNAME%は上目づかいで、 %CALLNAME:MASTER%を見上げている
		CASE 1
			PRINTFORML 「『きょーはいーひだな』」
			PRINTFORMW 「『だってたぼーなとーさまが、ぼくにじかんをさいてくれたんだか……ら……』」
			PRINTFORML 「って、もう。なにさ！　笑わないでよ」
			PRINTFORML 「こないだ見たお芝居でそんな台詞があって」
			PRINTFORMW 「いいなぁ、って思っただけなんだからさ。もうっ！」
		CASE 2
			PRINTFORML 「とーさま、来てくれたんだ。嬉しいな♪」
			PRINTFORML 「で、どうする？　訓練？　お喋り？　警邏？　それともえっちなこと？」
			PRINTFORMW 「うん、ボクはどれだっていーよ」
			PRINTFORML 「何れにしたってとーさまと一緒に過ごせるんだもの」
			PRINTFORMW 「ボクが厭う理由なんて、一欠けらだって存在しないよ」
		ENDSELECT
;既成事実Lv3(セックス済み)
ELSEIF MARK:2 == 3
	PRINTFORMW 「えっと、とーさま。今日もその……するの？」
;既成事実Lv2(恋人)
ELSEIF MARK:2 == 2
	PRINTFORMW 「ほら、はやくはやく。はやくしないと、とーさまと一緒の一日が早く終わっちゃうよ！」
	PRINTFORMW 出会いがしらに飛びついてきた%CALLNAME%はそう言って、%CALLNAME:MASTER%の腕に抱きついてきた
;既成事実Lv1(キス済み)
ELSEIF MARK:2 == 1
	PRINTFORMW 「とーさま、来てくれたんだ。今日はどうするの？」
;既成事実Lv0で恋慕
ELSEIF MARK:2 == 0 && TALENT:恋慕
	PRINTFORMW 「ねえねえとーさま。あのさ、あのさ……」
	PRINTFORMW 「えっと、あー……うーん……やっぱり、なんでもないや」
	PRINTFORMW %CALLNAME%は寂しそうな表情で、 %CALLNAME:MASTER%に笑って見せた。
;既成事実Lv0
ELSEIF MARK:2 == 0
	PRINTFORMW 「あ、とーさまだ。何しに来たの？」
ENDIF

;=================================================
;●「会いに行く」の終了時のセリフ
;=================================================
@KOJO_TRAIN_END_A1_K1400
;[虚ろ]状態なら口上を表示しない
IF TALENT:虚ろ
	RETURN
ENDIF

;-------------------------------------------------
;行動不能の場合
;-------------------------------------------------
;酒酔いによる行動不能
IF TCVAR:53 == 1
	PRINTFORMW 「むにゃ……もう呑めないよぅ……」
	PRINTFORMW %CALLNAME%は酒瓶を抱いて気持ちよさそうに眠っている
;快感のあまり気絶
ELSEIF TCVAR:52
	PRINTFORMW 「はっ……ひゃぅぅ……。とーさま、激しすぎだよ……」
	PRINTFORMW 聊か焦点の合わない瞳を虚空に彷徨わせ、%CALLNAME%は様々な体液に彩られた肢体をしどけなく横たえている
;疲労による行動不能
ELSEIF TCVAR:51
	PRINTFORMW 「うーん、つーかーれーたー」
	PRINTFORMW ばたんきゅーと態々口で言ってから、%CALLNAME%は倒れこんだ。幾ばくも待たぬうちに、穏やかな寝息が聞こえてくる
ENDIF

;-------------------------------------------------
;終了時(1回のみ)
;-------------------------------------------------
;既成事実Lv3(初めてセックスをした)
IF CFLAG:201 < 5 && MARK:2 == 3
	CFLAG:201 = 5
	;行動不能ならフラグだけ立てて表示はしない
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		PRINTFORMW 「えへへ。とうとう、きんしんそーかんしちゃったね」
	ENDIF
;既成事実Lv2(恋人になった)
ELSEIF CFLAG:201 < 4 && MARK:2 == 2
	CFLAG:201 = 4
	;行動不能ならフラグだけ立てて表示はしない
	IF !(TCVAR:51 || TCVAR:52 || TCVAR:53)
		PRINTFORML 「ふふ、嬉しいなぁ。とーさまが、とーさまってだけじゃなくて」
		PRINTFORMW 「別の意味でも、ボクの大切な人になってくれたんだもの……」
	ENDIF
;既成事実Lv1(初めてキスをした)
ELSEIF CFLAG:201 < 3 && MARK:2 == 1
	CFLAG:201 = 3
	;行動不能ならフラグだけ立てて表示はしない
