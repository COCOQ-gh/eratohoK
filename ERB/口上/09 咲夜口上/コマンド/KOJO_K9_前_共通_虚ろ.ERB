;虚ろ
@KOJO_K9_HOLLOW
#DIM 咲夜
咲夜 = NAME_TO_CHARA("咲夜")

SELECTCASE RAND:9
	CASE 1
		咲夜_入力台詞:0 = うー、
	CASE 2
		咲夜_入力台詞:0 = あ
	CASE 3
		咲夜_入力台詞:0 = ……ふ
	CASE 4
		咲夜_入力台詞:0 = ん……う
	CASE 5
		咲夜_入力台詞:0 = ふ
	CASE 6
		咲夜_入力台詞:0 = ……ん
	CASE 7
		咲夜_入力台詞:0 = ……は、あ
	CASE 8
		咲夜_入力台詞:0 = あー
	CASEELSE
		咲夜_入力台詞:0 = あう
ENDSELECT

SELECTCASE RAND:9
	CASE 1
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%うー
	CASE 2
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%あー……ぁ
	CASE 3
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%ぅふ
	CASE 4
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%あ
	CASE 5
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%ふ
	CASE 6
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%ぅん
	CASE 7
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%う
	CASE 8
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%あー
	CASEELSE
		咲夜_入力台詞:0 = %咲夜_入力台詞:0%あう
ENDSELECT

SELECTCASE RAND:6
	CASE 1
		咲夜_入力台詞:1 = ……あ
	CASE 2
		咲夜_入力台詞:1 = ……う
	CASE 3
		咲夜_入力台詞:1 = ……くぅ
	CASE 4
		咲夜_入力台詞:1 = ……は
	CASE 5
		咲夜_入力台詞:1 = ……ふ
	CASEELSE
		咲夜_入力台詞:1 = ……ん
ENDSELECT

SELECTCASE RAND:6
	CASE 1
		咲夜_入力台詞:1 = %咲夜_入力台詞:1%あ
	CASE 2
		咲夜_入力台詞:1 = %咲夜_入力台詞:1%う
	CASE 3
		IF 咲夜_入力台詞:1 != "く"
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%く
		ELSE
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%ふ
		ENDIF
	CASE 4
		IF 咲夜_入力台詞:1 != "は"
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%は
		ELSE
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%ふ
		ENDIF
	CASE 5
		咲夜_入力台詞:1 = %咲夜_入力台詞:1%ふ
	CASEELSE
		IF 咲夜_入力台詞:1 != "は"
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%ぁう
		ELSEIF 咲夜_入力台詞:1 != "ふ"
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%ん
		ELSE
			咲夜_入力台詞:1 = %咲夜_入力台詞:1%ぅん
		ENDIF
ENDSELECT

RETURN 0

