			IF CHECK_K10("陥落")
				PRINTDATA
					DATAFORM 「%CALLNAME_K10(レミリア_対象)%の腕が欲しいの❤」
					DATAFORM 「あ、あぐぅっ……き、つぅ、あっ❤」
					DATAFORM 「%CALLNAME_K10(レミリア_対象)%の腕ぇっ❤　あぁぁ、きついわっ」
					DATAFORM 「壊れちゃいそう……っ！　ふあぁ――っ❤　凄いっ」
				ENDDATA
			ELSE
				PRINTDATA
					DATAFORM 「もっとぉ。は、げしくっ。腕ごと挿れ、なさいっ……あ――っ」
					DATAFORM 「う、あぁあっ。そ、そう、もっと深く……あーっ」
					DATAFORM 「き、つう、あ、骨が……ひ、開くぅっ！　あはぁぁぁっ」
					DATAFORM 「あ――っ！　……こ、壊れそうっ。すごい、の。ああっ！」
				ENDDATA
			ENDIF
		ELSE
			IF CHECK_K10("陥落")
				PRINTDATA
					DATAFORM 「腕だめぇっ！　太いぃっ❤」
					DATAFORM 「きつぅ、いっ！　緩んじゃうっ！　うあぁっ！」
					DATAFORM 「%CALLNAME_K10(レミリア_対象)%の腕ぇっ❤　あぁぁっ！　きついわっ」
					DATAFORM 「壊れちゃいそう……っ！　ふあぁ――っ❤　凄いっ」
				ENDDATA
			ELSE
				PRINTDATA
					DATAFORM 「は、げしっ。腕ごと、なんてっ……あ――っ」
					DATAFORM 「う、あぁあっ！　深いぃっ！　……あーっ」
					DATAFORM 「き、つう、あ、骨が……ひ、開くぅっ！　あはぁぁぁっ」
					DATAFORM 「あ――っ！　……こ、壊れそうっ！　あああっ！」
				ENDDATA
			ENDIF
		ENDIF
	ENDIF
	RETURN 0
ENDIF

