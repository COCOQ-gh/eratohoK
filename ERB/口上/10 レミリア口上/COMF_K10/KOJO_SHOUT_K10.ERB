IF SELECTCOM == 94 || IS_EQUIP_TARGET(レミリア, 94)
	;口枷のとき
	IF SELECTCOM == 86 || IS_EQUIP_TARGET(レミリア, 86)
		PRINTDATA
			DATAFORM 「ふぶ、ず……っ！　んぶっ、う！」
			DATAFORM 「む、ぶっ……っ！　う～っ！」
			DATAFORM 「ふぐ……ぇっ……！　……！　うぶ……ッ」
			DATAFORM 「ふ……んぶぅっ……ん！　ぐふっ……ん……ッ！」
		ENDDATA
	;口枷じゃないとき
	ELSE
		;今回このコマンドでレミリアに主導権があればねだる台詞。求めてきたときの違和感低減
		IF IS_INITIATIVE(レミリア) && SELECTCOM == 94
			IF CHECK_K10("陥落")
				PRINTFORML 「首を絞めてくれる？　……別に今は、死後の世界に行きたいわけじゃないの」
				PRINTFORML 「%CALLNAME_K10(レミリア_対象)%にそうされてみたいって思ったのよ」
			ELSE
				PRINTFORML 「首を絞めてくれる？　別に死後の世界に行きたいわけじゃないの」
				PRINTFORML 「そうされてみたい気分なのよ」
			ENDIF
		ENDIF
		PRINTDATA
			DATAFORM 「あ……くっ！　……はぐっ、くふっ」
			DATAFORM 「う！　……っ、う……！　くふ……っ」
			DATAFORM 「ぐ……ぇっ……！　……！　かふ……ッ」
			DATAFORM 「うくぅっ……ん！　かふっ……！　ひゅっ」
		ENDDATA
	ENDIF
	RETURN 0
ENDIF

