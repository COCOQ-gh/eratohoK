@KOJO_K10_CANNOT_TALK
#DIM レミリア
#DIMS レミリア機嫌
レミリア = NAME_TO_CHARA("レミリア")
レミリア機嫌 = %TOSTR_EMOTION(レミリア)%

;口枷
IF SELECTCOM == 86 || IS_EQUIP_TARGET(レミリア, 86)
	SELECTCASE RAND:3
		CASE 1
			レミリア_入力台詞:0 = んごぉ、ふぐ
		CASE 2
			レミリア_入力台詞:0 = ふぐう
		CASEELSE
			レミリア_入力台詞:0 = んんっ、むっ。ふー……っ
	ENDSELECT
	SELECTCASE RAND:3
		CASE 1
			レミリア_入力台詞:1 = んむぅ
		CASE 2
			レミリア_入力台詞:1 = ふぶ、うぷっ
		CASEELSE
			レミリア_入力台詞:1 = ぉご、んんっ。むっ
	ENDSELECT
	GOTO END
ENDIF

;キス・ディープキス
IF SELECTCOM == 20 || SELECTCOM == 340 || SELECTCOM == 341 || SELECTCOM == 342 || IS_EQUIP(レミリア, 20, 340, 341, 342)
	;陥落しているか自分からしている時
	IF IS_SLAVE(レミリア) || IS_LOVER(レミリア) || IS_INITIATIVE(レミリア)
		SELECTCASE RAND:6
			CASE 1
				レミリア_入力台詞:0 = ちゅっ。んく
			CASE 2
				レミリア_入力台詞:0 = んれぇ
			CASE 3
				レミリア_入力台詞:0 = く、ふぁれぅ
			CASE 4
				レミリア_入力台詞:0 = あむっ。はむ、ん
			CASE 5
				レミリア_入力台詞:0 = んぇ
			CASEELSE
				レミリア_入力台詞:0 = ん
		ENDSELECT
		SELECTCASE RAND:6
			CASE 1
				レミリア_入力台詞:1 = あむ。ふ
			CASE 2
				レミリア_入力台詞:1 = ふっ
			CASE 3
				レミリア_入力台詞:1 = ふ、んく
			CASE 4
				レミリア_入力台詞:1 = んふ
			CASE 5
				レミリア_入力台詞:1 = んぷあ
			CASEELSE
				レミリア_入力台詞:1 = ちゅ
		ENDSELECT
		SELECTCASE RAND:6
			CASE 1
				レミリア_入力台詞:2 = ちゅっ……んく
			CASE 2
				レミリア_入力台詞:2 = れる、ちゅっ
			CASE 3
				レミリア_入力台詞:2 = く、ふぁ。れぅ
			CASE 4
				レミリア_入力台詞:2 = あむっ……はむ、ん
			CASE 5
				レミリア_入力台詞:2 = …………
			CASEELSE
				レミリア_入力台詞:2 = ん
		ENDSELECT
		SELECTCASE RAND:6
			CASE 1
				レミリア_入力台詞:3 = あむ。ふ
			CASE 2
				レミリア_入力台詞:3 = んふっ
			CASE 3
				レミリア_入力台詞:3 = はっ
			CASE 4
				レミリア_入力台詞:3 = ふ
			CASE 5
				レミリア_入力台詞:3 = ん。ぷ、ふあ
			CASEELSE
				レミリア_入力台詞:3 = ちゅ……はふっ
		ENDSELECT
	ELSE
		SELECTCASE RAND:6
			CASE 1
				レミリア_入力台詞:4 = ……う……
			CASE 2
				レミリア_入力台詞:4 = むっ……う
			CASE 3
				レミリア_入力台詞:4 = あぐ、んれぇ
			CASE 4
				レミリア_入力台詞:4 = ふ……んぐ
			CASE 5
				レミリア_入力台詞:4 = くっ……う、ん
			CASEELSE
				レミリア_入力台詞:4 = んくぅ……
		ENDSELECT
		SELECTCASE RAND:6
			CASE 1
				レミリア_入力台詞:4 = %レミリア_入力台詞:4%っ。嫌よ、ふ
			CASE 2
				レミリア_入力台詞:4 = %レミリア_入力台詞:4%、くふっ
			CASE 3
				レミリア_入力台詞:4 = %レミリア_入力台詞:4%ふ……んく
			CASE 4
				レミリア_入力台詞:4 = %レミリア_入力台詞:4%ん……
			CASE 5
				レミリア_入力台詞:4 = %レミリア_入力台詞:4%。ふうっ
			CASEELSE
				レミリア_入力台詞:4 = %レミリア_入力台詞:4%んぅ……
		ENDSELECT
	ENDIF
	GOTO END
ENDIF

