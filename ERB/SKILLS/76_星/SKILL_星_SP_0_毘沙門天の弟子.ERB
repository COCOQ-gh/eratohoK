;-----------------------------------
;基本値計算に先んじて処理するもの
;-----------------------------------
@SKILL_76_SP_0_EXIST
RETURN 1

@SKILL_76_SP_0_NAME
RESULTS = 毘沙門天の弟子

;レベルは1-5まで
@SKILL_76_SP_0_LEVEL
RETURN 4

;対象選択
@SKILL_76_SP_0_SETTARGET(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
#DIM 能力, 3
VARSET 能力

COMBAT_SKILL_TARGET = 発動番号
RETURN 1

;発動判定
@SKILL_76_SP_0_CAN_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
対象側 = !発動側
RETURN RAND:100 < 75

;効果をここに記述
@SKILL_76_SP_0_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
対象側 = !発動側
PRINTFORML この戦闘での武闘が上昇する！
BATTLE_武闘:発動側:COMBAT_SKILL_TARGET += 5
RETURN 1

;
@SKILL_76_SP_0_EXPLANATION
RESULTS = 自身の武闘が上昇。