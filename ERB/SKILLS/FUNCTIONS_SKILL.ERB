;---------------------------------
;SP, BASEスキル発動用
;---------------------------------
@SKILL_COMBAT_TRY_INVOKE(発動者, 発動番号, スキル, ジャンル, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象勢力
#DIM 対象部隊

;存在判定
TRYCCALLFORM SKILL_{発動者}_%ジャンル%_{スキル}_EXIST
	CATCH
		RETURN -1
ENDCATCH
;対象選択
TRYCCALLFORM SKILL_{発動者}_%ジャンル%_{スキル}_SETTARGET(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
	SIF !RESULT
		RETURN 0
CATCH
ENDCATCH
;発動判定
CALLFORM SKILL_{発動者}_%ジャンル%_{スキル}_CAN_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
SIF !RESULT
	RETURN 0
TRYCCALLFORM SKILL_{発動者}_%ジャンル%_{スキル}_INVOKE_TEXT
	CATCH
		CALLFORM SKILL_PRINT_INVOKE(発動者, スキル, ジャンル)
ENDCATCH
CALLFORM SKILL_{発動者}_%ジャンル%_{スキル}_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
RETURN RESULT



;---------------------------------
;スキル発動時のデフォルト地の文
;---------------------------------
@SKILL_PRINT_INVOKE(発動者, スキル, ジャンル)
#DIM 発動者
#DIM スキル
#DIMS ジャンル
CALLFORM SKILL_{発動者}_%ジャンル%_{スキル}_NAME
CALL COLORPRINTFORM(@"%ANAME(発動者)%のスキル「%RESULTS%」発動！", COLOR("スキル発動"), "L")