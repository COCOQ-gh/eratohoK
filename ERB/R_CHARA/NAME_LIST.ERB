;-------------------------
@SET_RANDOM_NAME
FOR LOCAL:0, 0, 3
	CALL SET_FAMILY_NAME
	CALL SET_FIRST_NAME

	;他のキャラと被った場合最大で３回まで再生成する
	LOCAL:2 = 1
	FOR LOCAL:1, 0, CHARANUM
		IF LOCAL:1 != TARGET && CSTR:0 == CSTR:(LOCAL:1):0 && CSTR:1 == CSTR:(LOCAL:1):1
			LOCAL:2 = 0
			BREAK
		ENDIF
	NEXT
	IF LOCAL:2
		BREAK
	ENDIF
NEXT

