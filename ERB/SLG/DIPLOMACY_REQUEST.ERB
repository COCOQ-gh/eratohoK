;外交で相手に要求をした場合の処理
;(ただし「降伏勧告」は DIPLOMACY_EACH.ERB)

;-------------------------------------------------
;要求時の共通処理
;ARG:1に1を渡すと、服従系素質を持つキャラに対して判定をスキップ
;要求が通った場合1、通らなかった場合0、処理をキャンセルした場合は-1を返す
;-------------------------------------------------
@DIPLOMACY_REQUEST_COMMON(ARG:0, ARG:1 = 0)
LOCAL:4 = GET_COUNTRY_BOSS(CFLAG:MASTER:1)
LOCAL:5 = GET_COUNTRY_BOSS(ARG:0)

;●武力による脅しが通るかどうかを判定
LOCAL:10 = 0

;勢力同士の隣接関係マップを作成
CALL TMP_CREATE_COUNTRY_NEIBORING_MAP

LOCAL:6 = GET_SUM_ECONOMY(CFLAG:MASTER:1)
LOCAL:7 = GET_SUM_ECONOMY(ARG:0)

LOCAL:8 = 0
FOR LOCAL:0, 1, MAX_COUNTRY
	SIF TMP_COUNTRY_IS_NEIBORING:(CFLAG:MASTER:1):(LOCAL:0)
		LOCAL:8 += GET_SUM_ECONOMY(LOCAL:0)
NEXT

;※条件は降伏勧告より緩和、引数で要求内容辺り参照して変化できるようにしたいなぁ
LOCAL:10 = DIPLOMACY_REQUEST_CHECK(CFLAG:MASTER:1, ARG:0, 10 + FLAG:99)
;こっそりヘイト加算
FLAG:99 ++

;既に要求を飲んだことがあり、前回と同様の方法が通用する場合
IF (LOCAL:10 && CFLAG:(LOCAL:5):42 == 1) || ((CFLAG:(LOCAL:5):42 == 2 && (TALENT:(LOCAL:5):烙印 || TALENT:(LOCAL:5):服従 || TALENT:(LOCAL:5):隷属 || (TALENT:(LOCAL:5):マゾ && TALENT:(LOCAL:5):恋慕)))) || (CFLAG:(LOCAL:5):42 == 3 && TALENT:(LOCAL:5):薬物依存)
	IF (CFLAG:(LOCAL:5):42 == 2) || (TALENT:(LOCAL:5):烙印 || TALENT:(LOCAL:5):服従 || TALENT:(LOCAL:5):隷属 || (TALENT:(LOCAL:5):マゾ && TALENT:(LOCAL:5):恋慕))
		PRINTFORMW %NAME:(LOCAL:5)%は困ったように瞳を彷徨わせながらも、媚びるような声で%ANAME(MASTER)%の要求を受け入れた
		CALL DIPLOMACY_REQUEST_GOHOUBI(ARG:0)
	ELSEIF CFLAG:(LOCAL:5):42 == 3 && TALENT:(LOCAL:5):薬物依存
		PRINTFORMW %NAME:(LOCAL:5)%は虚ろな瞳で、従うから薬をくれるよう%ANAME(MASTER)%に訴えてきた
		CALL DIPLOMACY_REQUEST_DRUG(ARG:0)
	ELSEIF TALENT:(LOCAL:5):反抗的 || TALENT:(LOCAL:5):生意気 || TALENT:(LOCAL:5):プライド高い
		PRINTFORMW %NAME:(LOCAL:5)%は%ANAME(MASTER)%を恨めしそうに睨みながら、怒気の篭った声で要求を受け入れた
	ELSE
		PRINTFORMW %NAME:(LOCAL:5)%は悲痛な表情を浮かべると、疲れたような声で%ANAME(MASTER)%の要求を受け入れた
	ENDIF
	RETURN 1
	DIPLOMACY_REQUEST_SUCCESS_COUNT ++

ELSEIF ARG:1 && (TALENT:(LOCAL:5):烙印 || TALENT:(LOCAL:5):服従 || TALENT:(LOCAL:5):隷属 || (TALENT:(LOCAL:5):マゾ && TALENT:(LOCAL:5):恋慕))
	IF TALENT:(LOCAL:5):烙印
		PRINTFORMW 既に%ANAME(MASTER)%に烙印を刻み付けられ所有物となった%NAME:(LOCAL:5)%は、断ることも出来ず頷いた
		PRINTFORMW ……心なしか嬉しそうに見える
		CFLAG:(LOCAL:5):42 = 2
		CALL DIPLOMACY_REQUEST_GOHOUBI(ARG:0)
	ELSE
		PRINTFORMW 既に%ANAME(MASTER)%に陶酔しきった%NAME:(LOCAL:5)%は、目を逸らしながらも頷いた
		PRINTFORMW ……心なしか嬉しそうに見える
		CFLAG:(LOCAL:5):42 = 2
		CALL DIPLOMACY_REQUEST_GOHOUBI(ARG:0)
	ENDIF
	DIPLOMACY_REQUEST_SUCCESS_COUNT ++
	RETURN 1

;要求を飲んだことがない or 前回の方法が通用しない場合
ELSE
	IF ARG:1 && TALENT:(LOCAL:5):恋慕
		PRINTFORML %NAME:(LOCAL:5)%は一瞬迷うように瞳を彷徨わせたが、
		PRINTFORMW すぐに我に返ったように首を振って、そんなこと出来るわけがないと断った
	ELSEIF TALENT:(LOCAL:5):反抗的 || TALENT:(LOCAL:5):生意気 || TALENT:(LOCAL:5):プライド高い
		PRINTFORMW ……だが、%NAME:(LOCAL:5)%は何の冗談かと笑い飛ばした
	ELSE
		PRINTFORMW ……だが、%NAME:(LOCAL:5)%はそんなこと出来るわけがないと強い口調で断った
	ENDIF
	LOCAL:11 = 0
	LOCAL:12 = 0
	PRINTFORML [0] 武力を背景に脅す
	IF TALENT:(LOCAL:5):烙印 || TALENT:(LOCAL:5):服従 || TALENT:(LOCAL:5):隷属 || (TALENT:(LOCAL:5):マゾ && TALENT:(LOCAL:5):恋慕)
		PRINTFORML [1] 快楽で言うことを聞かせる
		LOCAL:11 = 1
	ENDIF
	IF TALENT:(LOCAL:5):薬物依存
		PRINTFORML [2] 薬物で言うことを聞かせる
		LOCAL:12 = 1
	ENDIF
	PRINTFORML [3] 諦める…
	
	$INPUT_LOOP
	INPUT
	;武力で脅す
	IF RESULT == 0
		IF LOCAL:10
			PRINTFORMW %ANAME(MASTER)%は露骨に両国の武力の差をチラつかせながら交渉した
			IF TALENT:(LOCAL:5):反抗的 || TALENT:(LOCAL:5):生意気 || TALENT:(LOCAL:5):プライド高い
				PRINTFORMW %NAME:(LOCAL:5)%は%ANAME(MASTER)%の言葉に明らかに苛立っているが、圧倒的な戦力差を前に反論できずにいる
				PRINTFORMW ……やがて、%NAME:(LOCAL:5)%は怒りを押し殺した声で、要求に従うことを約束した
			ELSE
				PRINTFORMW %NAME:(LOCAL:5)%は%ANAME(MASTER)%の脅しも同然の言葉の前に、苦しげな表情で押し黙っている
				PRINTFORMW ……やがて、%NAME:(LOCAL:5)%は諦めたようにして、%ANAME(MASTER)%の要求に従うことを約束した
			ENDIF

			;関係悪化
			CALL CHANGE_RELATION_C_TO_C(ARG:0, CFLAG:MASTER:1, -100, 150)

			;性的な要求なら周囲からの評判も悪化
			IF ARG:1
				FOR LOCAL:0, 1, MAX_COUNTRY
					SIF IS_COUNTRY(LOCAL:0) && LOCAL:0 != CFLAG:MASTER:1
						CALL CHANGE_RELATION_C_TO_C(ARG:0, CFLAG:MASTER:1, -100, 50)
				NEXT
			ENDIF

			CFLAG:(LOCAL:5):42 = 1
			DIPLOMACY_REQUEST_SUCCESS_COUNT ++
			RETURN 1

		ELSE
			IF TALENT:(LOCAL:5):反抗的 || TALENT:(LOCAL:5):生意気 || TALENT:(LOCAL:5):プライド高い
				PRINTFORMW %ANAME(MASTER)%は両国の武力の差を熱心に語りかけたが、%NAME:(LOCAL:5)%は聞く耳を持たなかった…
			ELSE
				PRINTFORMW %ANAME(MASTER)%は両国の武力の差を熱心に語りかけたが、%NAME:(LOCAL:5)%に冷静に反論され話が進まなかった…
			ENDIF

			RETURN 0
		ENDIF

	;快楽で言うことを聞かせる
	ELSEIF RESULT == 1 && LOCAL:11
		;口上の表示
		PRINTFORMW %ANAME(MASTER)%は%NAME:(LOCAL:5)%に人払いをするように求めた
		PRINTFORMW ………………
		PRINTFORML %ANAME(MASTER)%は%NAME:(LOCAL:5)%の体を焦らすような手付きで愛撫しながら、
		PRINTFORMW 要求を飲んだらご褒美にたっぷり虐めてやると%NAME:(LOCAL:5)%の耳元で囁いた
		PRINTL 

		;●実行値の計算
		CVARSET TCVAR, 24, -10000
		CVARSET TCVAR, 25, 10000

		;実行値の設定
		TCVAR:(LOCAL:5):25 = 110

		LOCAL:2 = 0
		IF CFLAG:(LOCAL:5):4 < 1500
			LOCAL:2 = MAX(CFLAG:(LOCAL:5):4 / 100, 0)
		ELSE
			LOCAL:2 = MIN((CFLAG:(LOCAL:5):4 - 1500) / 500 + 15, 200)
		ENDIF
		CALL COM_ORDER_ELEMENT(LOCAL:5, "従属度", LOCAL:2)

		LOCAL:2 = 0
		IF CFLAG:(LOCAL:5):3 < 1500
			LOCAL:2 = MAX(CFLAG:(LOCAL:5):3 / 100, 0)
		ELSE
			LOCAL:2 = MIN((CFLAG:(LOCAL:5):3 - 1500) / 500 + 15, 200)
		ENDIF
		CALL COM_ORDER_ELEMENT(LOCAL:5, "依存度", LOCAL:2)

		IF TALENT:(LOCAL:5):隷属
			CALL COM_ORDER_ELEMENT(LOCAL:5, "隷属", 50)
		ELSEIF TALENT:(LOCAL:5):服従
			CALL COM_ORDER_ELEMENT(LOCAL:5, "服従", 25)
		ENDIF

		SIF TALENT:(LOCAL:5):烙印
			CALL COM_ORDER_ELEMENT(LOCAL:5, "烙印", 15)

		IF TALENT:(LOCAL:5):親愛
			CALL COM_ORDER_ELEMENT(LOCAL:5, "親愛", 20)
		ELSEIF TALENT:(LOCAL:5):恋慕
			CALL COM_ORDER_ELEMENT(LOCAL:5, "恋慕", 10)
		ENDIF

		SIF TALENT:(LOCAL:5):マゾ
			CALL COM_ORDER_ELEMENT(LOCAL:5, "マゾ", 20)

		CALL COM_ORDER_ELEMENT(LOCAL:5, "欲望", ABL:(LOCAL:5):欲望 * 2)

		SIF TALENT:(LOCAL:5):臆病
			CALL COM_ORDER_ELEMENT(LOCAL:5, "臆病", 8)
		SIF TALENT:(LOCAL:5):反抗的
			CALL COM_ORDER_ELEMENT(LOCAL:5, "反抗的", -5)
		SIF TALENT:(LOCAL:5):気丈
			CALL COM_ORDER_ELEMENT(LOCAL:5, "気丈", -10)
		SIF TALENT:(LOCAL:5):素直
			CALL COM_ORDER_ELEMENT(LOCAL:5, "素直", 5)
		SIF TALENT:(LOCAL:5):プライド高い
			CALL COM_ORDER_ELEMENT(LOCAL:5, "プライド高い", -15)
		SIF TALENT:(LOCAL:5):生意気
			CALL COM_ORDER_ELEMENT(LOCAL:5, "生意気", -5)
		SIF TALENT:(LOCAL:5):プライド低い
			CALL COM_ORDER_ELEMENT(LOCAL:5, "プライド低い", 10)
		SIF TALENT:(LOCAL:5):自制心
			CALL COM_ORDER_ELEMENT(LOCAL:5, "自制心", -15)
		SIF TALENT:(LOCAL:5):一線越えない
			CALL COM_ORDER_ELEMENT(LOCAL:5, "一線越えない", -10)

		;判定結果を表示
		PRINTFORMW  = {TCVAR:(LOCAL:5):24} %TOSTR_COMPARE(TCVAR:(LOCAL:5):24, TCVAR:(LOCAL:5):25)% 実行値{TCVAR:(LOCAL:5):25}

		;実行判定
		IF TCVAR:(LOCAL:5):24 >= TCVAR:(LOCAL:5):25
			PRINTFORMW ………………
			PRINTFORML しばらく愛撫を続けると、%NAME:(LOCAL:5)%はすっかり陶酔した表情になり、
			PRINTFORMW 媚びるような態度で%ANAME(MASTER)%の要求を飲むことを約束した
			CFLAG:(LOCAL:5):42 = 2
			DIPLOMACY_REQUEST_SUCCESS_COUNT ++
			RETURN 2
		ELSE
			PRINTFORML だが、いくら%ANAME(MASTER)%の頼みでもそれだけは応えることが出来ないと、
			PRINTFORMW %NAME:(LOCAL:5)%は申し訳なさそうに目を伏せて拒んだ
			RETURN 0
		ENDIF
	ELSEIF RESULT == 2 && LOCAL:12
		;口上の表示
		PRINTFORMW %ANAME(MASTER)%は%NAME:(LOCAL:5)%に人払いをするように求めた
		PRINTFORMW ………………
		PRINTFORML %ANAME(MASTER)%は%NAME:(LOCAL:5)%の眼前に錠剤をちらつかせながら、
		PRINTFORMW 要求を飲んだらとびきりキくやつをくれてやると%NAME:(LOCAL:5)%の耳元で囁いた
		PRINTL 
		IF CFLAG:(LOCAL:5):薬物依存 >= REL_HATE:(LOCAL:5):(MASTER)
			PRINTFORMW ………………
			PRINTFORML 一にも二にもなく、要求に従うことに%NAME:(LOCAL:5)%は同意した
			PRINTFORMW %ANAME(MASTER)%は褒美として、差し出された舌の上に錠剤を載せてやった……
			CFLAG:(LOCAL:5):42 = 3
			CFLAG:(LOCAL:5):薬物依存 += RAND(20, 50)
			DIPLOMACY_REQUEST_SUCCESS_COUNT ++
			RETURN 3
		ENDIF
	ENDIF
ENDIF

;諦める
RETURN -1

