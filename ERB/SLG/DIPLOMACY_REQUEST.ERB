IF ABL:(ARG:0):欲望 < 5
	LOCAL:2 = LOCAL:2 * (30 - ABL:(ARG:0):欲望) / 30
ELSE
	LOCAL:2 = LOCAL:2 * (130 - MIN(ABL:(ARG:0):欲望, 30)) / 150
ENDIF

LOCAL:3 = MAXBASE:(ARG:0):体力 * 100 / MAX(LOCAL:2, 1)
LOCAL:3 = MIN(LOCAL:3, TALENT:(ARG:0):絶倫 ? 40 # 20)

