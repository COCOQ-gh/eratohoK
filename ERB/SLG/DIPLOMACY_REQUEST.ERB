SELECTCASE ABL:(ARG:0):射精
	CASE 0
		TIMES LOCAL:2, 1.00
	CASE 1
		TIMES LOCAL:2, 0.80
	CASE 2
		TIMES LOCAL:2, 0.60
	CASE 3
		TIMES LOCAL:2, 0.45
	CASE 4
		TIMES LOCAL:2, 0.30
	CASEELSE
		LOCAL:2 = LOCAL:2 / (MIN(ABL:(ARG:0):射精 - 5, 55) + 5)
ENDSELECT

