;----------------------------------
;ARG:0勢力がARG:1勢力に行った要求が通るかの判定
;「(経済比)と(周辺国との経済比)の高い方」「兵力比」「トップ＋上位２名の政治パワー（トップ以外は好感度ブースト無し）」「要求を受けた側の感情値」で判定
;判定値をARG:2にて指定可能に。
;判定値に難易度による補正を追加、最大２倍
;※事前に TMP_CREATE_COUNTRY_NEIBORING_MAP 関数による隣接関係マップの作成が必要
;----------------------------------

@DIPLOMACY_REQUEST_CHECK(ARG:0, ARG:1, ARG:2 = 15)
#FUNCTION
#DIM ECONOMY, 3
#DIM ECONOMY_BORDER, 3
#DIM ARMY, 3
#DIM POLITICS, 3
#DIM POLITICS_SELF, 3
#DIM POLITICS_TARGET, 3
#DIM POWER_BALANCE, 5
#DIM BOSS, 2
#DIM RELATIONS

VARSET ECONOMY, 0
VARSET ARMY, 0
VARSET POLITICS_SELF, 0
VARSET POLITICS_TARGET, 0
VARSET ECONOMY_BORDER, 0
VARSET POWER_BALANCE, 0


;各カテゴリのパワー比の計算処理
;両勢力の経済力と比を計算
ECONOMY:1 = GET_SUM_ECONOMY(ARG:0), GET_SUM_ECONOMY(ARG:1)
ECONOMY:0 = ECONOMY:1 * 10 / ECONOMY:2

;周辺国との経済比を計算
FOR COUNT:1, 1, MAX_COUNTRY
	IF TMP_COUNTRY_IS_NEIBORING:(ARG:0):(COUNT:1)
		ECONOMY_BORDER:1 += GET_SUM_ECONOMY(COUNT:1)
		ECONOMY_BORDER:2 ++
	ENDIF
NEXT

;要求を突きつけた相手と隣接しており、自勢力が２勢力以上と隣接していれば周辺国との経済比を算出
SIF ECONOMY_BORDER:1 && TMP_COUNTRY_IS_NEIBORING:(ARG:0):(ARG:1) && ECONOMY_BORDER:2 > 1
	ECONOMY_BORDER:0 = ECONOMY:1 * 10 / ECONOMY_BORDER:1

;両勢力の兵数とその比を計算
ARMY:1 = GET_SUM_SOLDIER(ARG:0), GET_SUM_SOLDIER(ARG:1)
ARMY:0 = ARMY:1 * 10 / ARMY:2

BOSS:0 = GET_COUNTRY_BOSS(ARG:0), GET_COUNTRY_BOSS(ARG:1)

;勢力内でパワーの高い二人をそれぞれ探す（実際にいるのはパワーだけなので）
;相手君主との感情値で補正がかかるように（好意によって50～200%,敵意で25～100%が重複してかかる）
;対象勢力に所属し君主でなければ勢力ごとの能力配列POLITICS_X:2（最後尾）に上書きし、
;降順でソート（先頭に高い数値が入り最後尾が一番低くなる）
FOR COUNT:1, 0, CHARANUM
	SIF !IS_FREE(COUNT:1)
		CONTINUE
	;遊撃中もしくは特殊状態のキャラは除外
	IF CFLAG:(COUNT:1):1 == ARG:0 && BOSS:0 != COUNT:1
		;政治力の計算、ただし能力に感情値の補正が直接作用する
		POLITICS_SELF:2 = ABL_POWER(ABL:(COUNT:1):政治 * (REL_LIKE:(COUNT:1):(BOSS:1) / 10 + 50) * (100 - REL_HATE:(COUNT:1):(BOSS:1) / 20) / 10000)
;		POLITICS_SELF:2 = ABL_POWER(MAX(ABL:(COUNT:1):政治, ABL:(COUNT:1):武闘, ABL:(COUNT:1):知略))
		ARRAYSORT POLITICS_SELF, BACK
	ELSEIF CFLAG:(COUNT:1):1 == ARG:1 && BOSS:1 != COUNT:1
		;政治力の計算、ただし能力に感情値の補正が直接作用する
		POLITICS_TARGET:2 = ABL_POWER(ABL:(COUNT:1):政治 * (REL_LIKE:(COUNT:1):(BOSS:0) / 10 + 50) * (100 - REL_HATE:(COUNT:1):(BOSS:0) / 20) / 10000)
;		POLITICS_TARGET:2 = ABL_POWER(MAX(ABL:(COUNT:1):政治, ABL:(COUNT:1):武闘, ABL:(COUNT:1):知略))
		ARRAYSORT POLITICS_TARGET, BACK
	ENDIF
NEXT
;POLITICS_X:2にそれぞれの勢力の君主のパワーを入れる(君主間の感情値は判定要素として扱うので補正にかけない)
POLITICS_SELF:2 = ABL_POWER_X(ABL:(BOSS:0):政治, BOSS:0)
POLITICS_TARGET:2 = ABL_POWER_X(ABL:(BOSS:1):政治, BOSS:1)
;POLITICS_SELF:2 = ABL_POWER_X(ABL:(BOSS:0):政治 * (REL_LIKE:(BOSS:0):(BOSS:1) / 10 + 50) * (100 - REL_HATE:(BOSS:0):(BOSS:1) / 20) / 10000, BOSS:0)
;POLITICS_TARGET:2 = ABL_POWER_X(ABL:(BOSS:1):政治 * (REL_LIKE:(BOSS:1):(BOSS:0) / 10 + 50) * (100 - REL_HATE:(BOSS:1):(BOSS:0) / 20) / 10000, BOSS:1)
;POLITICS_SELF:2 = ABL_POWER_X(MAX(ABL:GET_COUNTRY_BOSS(ARG:0):政治, ABL:GET_COUNTRY_BOSS(ARG:0):武闘, ABL:GET_COUNTRY_BOSS(ARG:0):知略), GET_COUNTRY_BOSS(ARG:0))
;POLITICS_TARGET:2 = ABL_POWER_X(MAX(ABL:GET_COUNTRY_BOSS(ARG:1):政治, ABL:GET_COUNTRY_BOSS(ARG:1):武闘, ABL:GET_COUNTRY_BOSS(ARG:1):知略), GET_COUNTRY_BOSS(ARG:1))

;RELATIONS = REL_LIKE:(BOSS:1):(BOSS:0) - REL_HATE:(BOSS:1):(BOSS:0)

DEBUGPRINTFORML 【戦力】：{POLITICS_SELF:0} / {POLITICS_SELF:1} / {POLITICS_SELF:2}
DEBUGPRINTFORML 【戦力】：{POLITICS_TARGET:0} / {POLITICS_TARGET:1} / {POLITICS_TARGET:2}

POLITICS:1 =  SUMARRAY(POLITICS_SELF)
POLITICS:2 =  SUMARRAY(POLITICS_TARGET)
POLITICS:0 = POLITICS:1 * 10 / POLITICS:2

;カテゴリごとに判定（比は割=10%単位で算出）
;なお、相手のほうが規模が大きければ印象が悪くなるのは当然
;二国間の経済比
SELECTCASE ECONOMY:0
CASE 0
	POWER_BALANCE:1 = -3
CASE IS < 10
	POWER_BALANCE:1 = -1
CASE IS < 30
	POWER_BALANCE:1 = 0
CASE IS < 50
	POWER_BALANCE:1 = 1
CASE IS < 100
	POWER_BALANCE:1 = 2
CASE IS < 150
	POWER_BALANCE:1 = 4
CASE IS < 200
	POWER_BALANCE:1 = 7
CASE IS >= 200 
	POWER_BALANCE:1 = 10
CASEELSE
ENDSELECT

;周辺国と要求国との経済比、二国間との経済比によるものと良い方を使用
SELECTCASE ECONOMY_BORDER:0
CASE 0
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, -3)
CASE IS < 10
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, -1)
CASE IS < 20
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, 0)
CASE IS < 30
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, 1)
CASE IS < 50
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, 2)
CASE IS < 75
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, 4)
CASE IS < 100
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, 7)
CASE IS >= 100 
	POWER_BALANCE:1 = MAX(POWER_BALANCE:1, 10)
CASEELSE
ENDSELECT

;兵力比
SELECTCASE ARMY:0
CASE 0
	POWER_BALANCE:2 = -3
CASE IS < 10
	POWER_BALANCE:2 = -1
CASE IS < 20
	POWER_BALANCE:2 = 0
CASE IS < 40
	POWER_BALANCE:2 = 1
CASE IS < 60
	POWER_BALANCE:2 = 2
CASE IS < 80
	POWER_BALANCE:2 = 4
CASE IS < 100
	POWER_BALANCE:2 = 7
CASE IS >= 100
	POWER_BALANCE:2 = 10
CASEELSE
ENDSELECT

;政治力比
SELECTCASE POLITICS:0
CASE 0
	POWER_BALANCE:3 = -3
CASE IS < 25
	POWER_BALANCE:3 = -1
CASE IS < 60
	POWER_BALANCE:3 = 0
CASE IS < 120
	POWER_BALANCE:3 = 1
CASE IS < 200
	POWER_BALANCE:3 = 2
CASE IS < 300
	POWER_BALANCE:3 = 4
CASE IS < 500
	POWER_BALANCE:3 = 7
CASE IS >= 500
	POWER_BALANCE:3 = 10
CASEELSE
ENDSELECT

;感情値
;SELECTCASE RELATIONS
;CASE IS < -500
;	POWER_BALANCE:3 = -3
;CASE IS < -200
;CASE IS < 0
;	POWER_BALANCE:3 = 1
;CASE IS < 300
;	POWER_BALANCE:3 = 2
;CASE IS < 500
;	POWER_BALANCE:3 = 4
;CASE IS < 800
;	POWER_BALANCE:3 = 7
;CASE IS >= 800
;	POWER_BALANCE:3 = 10
;CASEELSE
;ENDSELECT

POWER_BALANCE:0 = SUMARRAY(POWER_BALANCE)

LOCAL:0 = ARG:0 == CFLAG:MASTER:1 ? ((CONFIG:300 + 1) * 5) # 10
LOCAL:1 = ARG:2 * LOCAL:0 / 10

DEBUGPRINTFORML 【経済比】{POWER_BALANCE:1}, 【戦力比】{POWER_BALANCE:2}, 【能力比】{POWER_BALANCE:3}
;DEBUGPRINTFORML 【外交要求判定】%NAME:(BOSS:0)%→%NAME:(BOSS:1)%：判定値{POWER_BALANCE:0}/難易度{MAX(ARG:2, 2)}
DEBUGPRINTFORML 【外交要求判定】%NAME:(BOSS:0)%→%NAME:(BOSS:1)%：判定値:{POWER_BALANCE:0}/難易度:{LOCAL:1}

;LOCAL:2 = POWER_BALANCE >= 2 ? LOCAL:1 < RAND:POWER_BALANCE # 0
RETURNF LOCAL:1 < (POWER_BALANCE > 0 ? RAND:POWER_BALANCE # 0)


;-----------------------------------------------------------------------------------------------------
;ARG:0勢力の強要行為に対する反発効果の処理
;軽:好意減少及び敵意増大
;小:条約／連合破棄（相手側の敵意で判定）、一時的外交停止
;中:一時的外交停止
;大:反連合の成立
;返り値 0:お咎め無し, 1-15:有罪、何らかのペナルティが発生, -1:冤罪が判明したので無罪放免
;-----------------------------------------------------------------------------------------------------

@DIPLOMACY_BACKFIRE(ARG:0)
;上から順に
;返り値用のペナルティ処理計測値、勢力トップのキャラ番号、勢力のAIタイプ、ループ処理用のカウンタ
;同盟／停戦／連合の破棄を決めた勢力
#DIM RES_PENALTY
#DIM BOSS_CHARA, 2
#DIM HATE, 2
#DIM BOSS_AI, 2
#DIM LOOP_COUNT, 3
#DIM HATE_A, MAX_COUNTRY
#DIM HATE_C, MAX_COUNTRY
#DIM HATE_U, MAX_COUNTRY

;DIPLOMACY_HATE:(ARG:0)
;範囲外、チャンネルが閉じている勢力は冤罪のため釈放
SIF !INRANGE(ARG:0, 1, MAX_COUNTRY) || COUNTRY_IS_CLOSED:(ARG:0)
	RETURN -1
VARSET RES_PENALTY
VARSET BOSS_CHARA
VARSET HATE
VARSET LOOP_COUNT
VARSET HATE_A
VARSET HATE_C
VARSET HATE_U

;大賞勢力の基礎情報を取得
BOSS_CHARA:0 = GET_COUNTRY_BOSS(ARG:0)
BOSS_AI:0 = COUNTRY_AI_TYPE:(ARG:0)

;軽ペナルティ:好感度減少及び敵対値増大
IF DIPLOMACY_HATE:(ARG:0):0 >= DIPLOMACY_HATE:(ARG:0):1 + 10
	SETCOLOR COLOR("警告")
	PRINTFORMW 度重なる他勢力への要求により、%ANAME(BOSS_CHARA:0)%を警戒する動きが各国で出ているようです！
	PRINTFORMW %ANAME(BOSS_CHARA:0)%と他国との関係が悪化しました
	RESETCOLOR
	FOR LOOP_COUNT:0, 0, MAX_COUNTRY
		IF IS_COUNTRY(LOCAL:0) && LOOP_COUNT:0 != ARG:0 && !COUNTRY_IS_CLOSED:(LOOP_COUNT:0)
			BOSS_AI:1 = COUNTRY_AI_TYPE:(LOOP_COUNT:0)
			HATE:0 = -1 * RAND(DIPLOMACY_HATE:(ARG:0):1 * 5, DIPLOMACY_HATE:(ARG:0):1 * 10) * AI_RELATION:(BOSS_AI:1):(BOSS_AI:0) / 100
			HATE:1 = RAND(DIPLOMACY_HATE:(ARG:0):1 * 5, DIPLOMACY_HATE:(ARG:0):1 * 10) * 100 / AI_RELATION:(BOSS_AI:1):(BOSS_AI:0)
			CALL CHANGE_RELATION_C_TO_C(LOOP_COUNT:0, ARG:0, HATE:0, HATE:1)
		ENDIF
	NEXT
	RES_PENALTY += 1
	DIPLOMACY_HATE:(ARG:0):1 = DIPLOMACY_HATE:(ARG:0):0 + 10
ENDIF

;小ペナルティ:相手の持つ敵対値に応じて条約強制破棄／連合からの排除の実施
IF DIPLOMACY_HATE:(ARG:0):0 >= DIPLOMACY_HATE:(ARG:0):2 + 20
	SETCOLOR COLOR("警告")
	PRINTFORMW 度重なる他勢力への圧力により、%ANAME(BOSS_CHARA:0)%に反発する動きが各勢力で出ているようです！
	;破棄をする勢力のチェック

	;面倒なので同盟停戦連合一括処理(ぉ
	FOR LOOP_COUNT:0, 0, MAX(MAX_TREATY_A, MAX_TREATY_C, MAX_TREATY_U)
		;同盟のチェック
		IF LOOP_COUNT:0 < MAX_TREATY_A
			FOR LOOP_COUNT:1, 0, MAX_TREATY_COUNTRY
				IF TREATY_A_COUNTRY:(LOOP_COUNT:0):(LOOP_COUNT:1)
					;参加国リストに問題の勢力が存在すれば該当する同盟を参加国のチェックを行う
					IF TREATY_A_COUNTRY:(LOOP_COUNT:0):(LOOP_COUNT:1) == ARG:0
						FOR LOOP_COUNT:2, 0, MAX_TREATY_COUNTRY
						NEXT
					ENDIF
				ELSE
					BREAK
				ENDIF
			NEXT
		ENDIF
		;停戦条約のチェック
		IF LOOP_COUNT:0 < MAX_TREATY_C
			FOR LOOP_COUNT:1, 0, MAX_TREATY_COUNTRY
				IF TREATY_C_COUNTRY:(LOOP_COUNT:0):(LOOP_COUNT:1)
				ELSE
					BREAK
				ENDIF
			NEXT
		ENDIF
		;連合のチェック
		IF LOOP_COUNT:0 < MAX_TREATY_U
			FOR LOOP_COUNT:1, 0, MAX_TREATY_COUNTRY
				IF TREATY_U_COUNTRY:(LOOP_COUNT:0):(LOOP_COUNT:1)
				ELSE
					BREAK
				ENDIF
			NEXT
		ENDIF
	NEXT
	RESETCOLOR

	RES_PENALTY += 2
	DIPLOMACY_HATE:(ARG:0):2 = DIPLOMACY_HATE:(ARG:0):0 + 20
ENDIF

;中ペナルティ:一時的な外交停止
IF DIPLOMACY_HATE:(ARG:0):0 >= DIPLOMACY_HATE:(ARG:0):3 + 40
	PRINTFORMW %ANAME(BOSS_CHARA:0)%との外交を拒否する動きが各国にあるようです
	;大体ペナルティ発動回数ターンだけ他勢力と外交ができなくなる
	COUNTRY_NOTDIPLOMACY_TERM:(ARG:0) += DIPLOMACY_HATE:(ARG:0):0 * (80 + RAND:21 + RAND:21) / (100 * 40)
	RES_PENALTY += 4
	DIPLOMACY_HATE:(ARG:0):3 = DIPLOMACY_HATE:(ARG:0):0 + 40
	SETCOLOR COLOR("警告")

ENDIF

;大ペナルティ:一定以上の敵対値をもつ勢力が連合を組む（これにより他の連合は全てリセットされる）
IF DIPLOMACY_HATE:(ARG:0):0 >= DIPLOMACY_HATE:(ARG:0):4 + 60
	PRINTFORMW %ANAME(BOSS_CHARA:0)%の横暴な外交姿勢に対し他国の我慢は限界を超えてしまいました！

	RES_PENALTY += 8
	DIPLOMACY_HATE:(ARG:0):4 = DIPLOMACY_HATE:(ARG:0):0 + 60
	SETCOLOR COLOR("警告")

ENDIF

RETURN RES_PENALTY
