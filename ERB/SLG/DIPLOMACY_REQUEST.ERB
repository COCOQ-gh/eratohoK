			CFLAG:(LOCAL:30):9 = CFLAG:MASTER:1
			;部隊に含まれているなら取り除く
			FOR LOCAL, 0, MAX_UNIT
				FOR LOCAL:1 , 0, 3
					SIF LOCAL:30 == GET_UNIT_COMMANDER(ARG:0, LOCAL, LOCAL:1)
						CALL CLEAR_UNIT_COMMANDER(ARG:0, LOCAL, LOCAL:1)
				NEXT
			NEXT
			;都市の守将でも取り除く
			FOR LOCAL, 0, MAX_CITY
				FOR LOCAL:1, 0, 2
					SIF LOCAL:30 == GET_CITY_COMMANDER(LOCAL, LOCAL:1)
						CALL CLEAR_CITY_COMMANDER(LOCAL, LOCAL:1)
				NEXT
			NEXT
			REDRAW 1
			;快楽で言うことを聞かせた場合、ご褒美の処理
			SIF LOCAL:2 == 2
				CALL DIPLOMACY_REQUEST_PRIZE(ARG:0)
			;薬物で言うことを聞かせた場合、ドラッグの処理
			SIF LOCAL:2 == 3
				CALL DIPLOMACY_REQUEST_DRUG(ARG:0)
			RETURN 1
		ELSE
			REDRAW 1
			GOTO SHOW_LOOP1
		ENDIF
	ENDIF
ENDIF
CLEARLINE 1
GOTO INPUT_LOOP

