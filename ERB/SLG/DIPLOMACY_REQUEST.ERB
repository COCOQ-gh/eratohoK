		SIF TALENT:(LOCAL:0):烙印 || TALENT:(LOCAL:0):服従 || TALENT:(LOCAL:0):隷属 || (TALENT:(LOCAL:0):マゾ && TALENT:(LOCAL:0):恋慕) && CFLAG:(LOCAL:0):42 == 2
			LOCAL:4 = 1

		SIF CFLAG:(LOCAL:0):薬物依存 >= REL_HATE:(LOCAL:0):(MASTER) && CFLAG:(LOCAL:0):43 == 3
			LOCAL:4 = 1

		;【条件A】【条件B】のいずれも満たさず2ターン以降経過しているなら解除
		IF !LOCAL:3 && !LOCAL:4 && CFLAG:(LOCAL:0):59 > 2
			CFLAG:(LOCAL:0):59 = 0
			PRINTL 
			PRINTFORML %ANAME(LOCAL:0)%はこれ以上%ANAME(MASTER)%に体を許すつもりはないと通告してきた…
			SETCOLOR COLOR("注意")
			PRINTFORMW %ANAME(LOCAL:0)%が「捕虜の調教」の調教対象から外れました…
			RESETCOLOR
			CONTINUE
		ENDIF
		
		SIF CFLAG:(LOCAL:0):59
			CFLAG:(LOCAL:0):59 ++
	ENDIF
NEXT

