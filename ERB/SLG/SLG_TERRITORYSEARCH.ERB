;-------------------------------------------------
;領地探索
;-------------------------------------------------
@TERRITORYSEARCH
#DIM 対象
#DIM 金額
#DIM 兵数
#DIM 候補キャラ, 1000
#DIM 候補数
#DIM 変化値
VARSET 候補キャラ
VARSET 候補数

DRAWLINE
DONE_TERRITORYSEARCH = 1
CALL SET_COOLTIME(MASTER, 1)
PRINTFORMW 領地内を探索することにした
PRINTFORML ・
PRINTFORML ・
PRINTFORMW ・
SIF RAND:3 >= 1
	PRINTFORMW 特にめぼしい物は見つからなかった
SELECTCASE RAND:32
	;妖怪を討伐する
	CASE 0
		PRINTFORMW 領民によれば近頃この辺りで妖怪が暴れて困っているらしい
		PRINTFORMW 何とかしてくれと頼まれた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"討伐を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 危険な相手だ、領民には悪いが自分にはどうしようもない
				PRINTFORMW そう思った%ANAME(MASTER)%は宮殿へ戻った
			ELSE
				PRINTFORMW そんな危険な奴は放置できない！
				PRINTFORMW %ANAME(MASTER)%は武器を持って妖怪の巣へと乗り込んだ
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:MASTER:武闘) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(MASTER)%は見事妖怪を討伐することに成功した
					PRINTFORMW 領民に感謝されて宮殿へ戻った
					CALL PRINT_ADD_EXP(MASTER, "武闘経験値", RAND:3 + 1, 1)
				ELSE
					IF IS_MALE(MASTER)
						PRINTFORMW やられた！
						PRINTFORMW 妖怪は想像以上に手強く、返り討ちにあった%ANAME(MASTER)%は命からがら逃げかえった
						CALL ADD_COOLTIME(MASTER, 3)
						PRINTFORMW %ANAME(MASTER)%は3ターンのクールダウンに入った
					ELSE
						PRINTFORMW やられた！
						PRINTFORMW 妖怪は想像以上に手強く、%ANAME(MASTER)%は返り討ちにされて巣へと引きずられていった
						PRINTFORML 
						PRINTFORMW %ANAME(MASTER)%は妖怪の交尾相手にされ、野生的なセックスで何度も種付けされてしまった
						CALL FORCE_FUCK(MASTER, -1, @"野良妖怪の\@RAND:2 ? ペニス # 唇\@", @"野良妖怪", RAND:2 + 1)
						PRINTFORML 
						PRINTFORMW 散々犯された後、何とか逃げ帰った
						CALL ADD_COOLTIME(MASTER, 3)
						PRINTFORMW %ANAME(MASTER)%は3ターンのクールダウンに入った
					ENDIF
				ENDIF
			ENDIF
	;つちのこを捕獲する
	CASE 1
		PRINTFORMW 領民によればこの辺りでつちのこを見た者がいるらしい
		PRINTFORMW 賞金も出ているので捕まえればちょっとした小遣いにもなりそうだ
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"捕獲を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW ヘビの見間違いか何かだろう
				PRINTFORMW そう思った%ANAME(MASTER)%は宮殿へ戻った
			ELSE
				PRINTFORMW 面白そうだ
				PRINTFORMW %ANAME(MASTER)%も知恵を絞って罠を張り、捕獲を試みた
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:MASTER:知略) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(MASTER)%は見事つちのこを捕獲することに成功した
					PRINTFORMW 賞金を手に入れ、ホクホク顔で宮殿へ戻った
					金額 = MIN(DAY * 50, 5000) + RAND(1000, 5000)
					MONEY += 金額
					PRINTFORMW 金{金額}を手に入れた
				ELSE
					PRINTFORMW やられた！
					PRINTFORMW 餌だけを奪われてしまった
					PRINTFORMW 本当につちのこがいたのかもわからぬまま、悔しがりながら%ANAME(MASTER)%は宮殿へ戻った
				ENDIF
			ENDIF
	;古びた矛を拾う
	CASE 2
		PRINTFORMW 探索中、古びた矛を見つけた
		PRINTFORMW 長い間放置されていた様だが、磨けばまだ使えるかもしれない
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"持ち帰って磨く", "武器屋に売る", "捨てておく")
			IF RESULT == 2
				PRINTFORMW こんなボロボロの武器には用はない
				PRINTFORMW そう思った%ANAME(MASTER)%は矛を放り投げると宮殿へ戻った
			ELSEIF RESULT == 1
				PRINTFORMW こんなものでも武器だ、もしかしたら高値で売れるかもしれない
				PRINTFORMW %ANAME(MASTER)%は武器屋へ持って行った
				金額 = MIN(3000 + 1000 * (DAY / 10), 10000)
				PRINTFORMW …矛は金{金額}で売れた
				PRINTFORMW 思わぬ臨時収入に%ANAME(MASTER)%はホクホク顔で宮殿へ戻った
				MONEY += 金額
			ELSE
				PRINTFORMW 折角拾った武器だ、持ち帰ってみよう
				PRINTFORMW そう思った%ANAME(MASTER)%は矛を抱えて宮殿へ戻った
				PRINTFORMW 宮殿に戻ると早速矛を磨いてみた
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF RAND:3 == 0
					PRINTFORMW すると矛は立派な輝きを取り戻した！
					PRINTFORMW 手に持ってみると力がみなぎるのを感じた
					CALL PRINT_ADD_EXP(MASTER, "武闘経験値", RAND:3 + 1, 1)
				ELSE
					PRINTFORMW そこら辺にあるような普通の矛だった
					PRINTFORMW 自分には不要だと思い、部下に与えることにした
				ENDIF
			ENDIF
	;山賊を討伐する
	CASE 3
		PRINTFORMW 領民によれば近頃この辺りで山賊が出没して困っているらしい
		PRINTFORMW 何とかしてくれと頼まれた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"討伐を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 危険な相手だ、領民には悪いが自分にはどうしようもない
				PRINTFORMW そう思った%ANAME(MASTER)%は宮殿へ戻った
			ELSE
				PRINTFORMW そんな危険な連中は放置できない！
				PRINTFORMW %ANAME(MASTER)%は山賊を捕まえる為の罠を張った
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:MASTER:知略) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(MASTER)%は見事山賊を討伐することに成功した！
					PRINTFORMW 領民に感謝されて宮殿へ戻った
					CALL PRINT_ADD_EXP(MASTER, "知略経験値", RAND:3 + 1, 1)
				ELSE
					IF IS_MALE(MASTER)
						PRINTFORMW やられた！
						PRINTFORMW 山賊は想像以上に手強く、返り討ちにあった%ANAME(MASTER)%は命からがら逃げかえった
						CALL ADD_COOLTIME(MASTER, 3)
						PRINTFORMW %ANAME(MASTER)%は3ターンのクールダウンに入った
					ELSE
						PRINTFORMW やられた！
						PRINTFORMW 山賊は想像以上に手強く、%ANAME(MASTER)%は返り討ちにされてねぐらへと引きずられていった
						PRINTFORML 
						PRINTFORMW %ANAME(MASTER)%は山賊どもの慰み者としてボロボロになるまで輪姦されてしまった
						CALL FORCE_FUCK(MASTER, -1, @"山賊の\@RAND:2 ? ペニス # 唇\@", @"山賊", RAND:2 + 1)
						PRINTFORML 
						PRINTFORMW 散々犯された後、何とか逃げ帰った
						CALL ADD_COOLTIME(MASTER, 3)
						PRINTFORMW %ANAME(MASTER)%は3ターンのクールダウンに入った
					ENDIF
				ENDIF
			ENDIF
	;お祭りを楽しむ
	CASE 4
		PRINTFORMW 領内で盛大にお祭りが開かれていた
		PRINTFORMW 久しぶりの息抜きに、%ANAME(MASTER)%も大いに楽しんだ
		金額 = MIN(5000 * (DAY / 10), 10000)
		SIF 金額 > MONEY
			金額 = MONEY / 2
		MONEY -= 金額
		PRINTFORMW {金額}を消費した
	;茸を見つける
	CASE 5
		PRINTFORMW 森の中で不思議な形をした茸を見つけた
		PRINTFORMW 以前図鑑で見たことがある、これは珍味の茸だ！
		PRINTFORMW 毒があるらしいがきちんと調理すればとても美味らしい
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"調理を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 素人が下手に手を出すべきじゃない
				PRINTFORMW そう思った%ANAME(MASTER)%は宮殿へ戻った
			ELSE
				PRINTFORMW 危険かもしれないが食欲には勝てなかった
				PRINTFORMW %ANAME(MASTER)%は茸を持ち帰り調理した
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:MASTER:料理) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW 美味しい！
					PRINTFORMW 茸はまるで上等な霜降りの様に舌の上で蕩け、%ANAME(MASTER)%は極上の味に思わず頬が緩んだ
					PRINTFORMW %ANAME(MASTER)%は料理に自信を持った
					CALL PRINT_ADD_EXP(MASTER, "料理経験値", RAND:3 + 1, 1)
				ELSE
					PRINTFORMW しまった！
					PRINTFORMW 調理に失敗した%ANAME(MASTER)%は腹をこわしてしまった
					PRINTFORMW 教訓にしよう…お腹を押さえて冷や汗をかきながら%ANAME(MASTER)%はそう強く思った
					CALL ADD_COOLTIME(MASTER, 3)
					PRINTFORMW %ANAME(MASTER)%は3ターンのクールダウンに入った
				ENDIF
			ENDIF
	;水浴びを愉しむ
	CASE 6
		PRINTFORMW 探索中、森の奥で水浴びに丁度良い水場を見つけた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI_JUDGE(@"水浴びをする", 1,"水浴びを覗く", IS_MALE(MASTER),"水を飲む", 1)
			IF RESULT == 2
				PRINTFORMW 水はとても冷たく、飲むと力が湧いてきた
				PRINTFORMW 喉を潤した%ANAME(MASTER)%は充足して宮殿へ戻った
			ELSEIF RESULT == 1
				PRINTFORMW 誰かが水浴びをしている様だ
				PRINTFORMW %ANAME(MASTER)%はニヤリと笑い見つからない様に茂みに隠れながら覗きを敢行した
				PRINTFORML 
				PRINTFORMW 水浴びをしていたのは人間の娘だった
				PRINTFORMW まだ若さが残るものの凹凸のはっきりとした女らしい肉体がこの距離でもわかる
				PRINTFORMW 覗いているとムラムラしてきた
				PRINTFORMW どうしよう？
				CALL ASK_MULTI(@"襲いかかる", "覗き続ける")
					IF RESULT == 1
						PRINTFORMW %ANAME(MASTER)%は見つからない様にしながらもその魅力的な肉体を目に焼き付けた
					ELSE
						PRINTFORMW %ANAME(MASTER)%は周囲に他に人の気配がない事を確認すると
						PRINTFORMW 茂みを飛び出し、水浴びをしている少女に襲いかかった！
						PRINTFORMW 少女は突然の暴漢に驚き逃げようとしたが、足を滑らせ捕まってしまった
						PRINTFORMW %ANAME(MASTER)%は暴れる彼女を無理矢理茂みの中へと連れ込んだ
						PRINTFORML 
						PRINTFORMW 娘の膣は非常によく締まり、%ANAME(MASTER)%は我慢できず何度も精を放った
						PRINTFORML 
						PRINTFORMW 茫然と横たわっている娘を放置して%ANAME(MASTER)%は宮殿へ戻った
					ENDIF
			ELSE
				PRINTFORMW 折角だから水浴びをしていくことにした
				PRINTFORML 
				IF IS_MALE(MASTER)
					PRINTFORMW とても気持ちがよかった
					PRINTFORMW さっぱりした%ANAME(MASTER)%は充足して宮殿へ戻った
				ELSE
					PRINTFORMW %ANAME(MASTER)%が水浴びをしているとどこからか視線を感じた
					PRINTFORMW どうしよう？
					CALL ASK_MULTI(@"水浴びを続ける", "水から上がる")
						IF RESULT == 1
							PRINTFORMW 不穏な空気を察した%ANAME(MASTER)%は
							PRINTFORMW 早々に水浴びを切り上げると、足早に宮殿へ戻った
						ELSE
							PRINTFORMW 気のせいだと水浴びを続行した
							PRINTFORMW すると突然近くの茂みから痴漢が飛び出し、襲いかかってきた！
							PRINTFORMW 抵抗する%ANAME(MASTER)%だったが、裸の状態では何もできず、無理矢理茂みの中へと連れ込まれた
							PRINTFORML 
							PRINTFORMW %ANAME(MASTER)%は四つん這いに押し倒され、痴漢によって何度も汚されてしまった
							CALL FORCE_FUCK(MASTER, -1, @"痴漢の\@RAND:2 ? ペニス # 唇\@", @"痴漢", RAND:2 + 1)
							PRINTFORML 
							PRINTFORMW 痴漢は秘所から白濁液を垂らしながら横たわっている%ANAME(MASTER)%を放置して去って行った
						ENDIF
				ENDIF
			ENDIF
	;迷子と遭遇する
	CASE 7
		FOR LOCAL, 0, CHARANUM
			IF CFLAG:(LOCAL):所属 == CFLAG:MASTER:所属 && !CFLAG:(LOCAL):9 && !IS_ANIMAL(LOCAL)
				候補キャラ:LOCAL = RAND:1000 
				候補数 ++
			ENDIF
		NEXT
		IF 候補数 == 0
			対象 = 0
		ELSE
			対象 = FINDELEMENT(候補キャラ, MAXARRAY(候補キャラ))
		ENDIF
		PRINTFORMW 領内を探索中、迷子の男の子と遭遇した
		PRINTFORMW 親とはぐれてしまったらしいが、周囲にそれらしい人物は見当たらない
		PRINTFORMW どうしようか？
		CALL ASK_MULTI_JUDGE(@"一緒に親を探す", 1,"誘惑する", IS_FEMALE(MASTER) && TALENT:MASTER:処女 != 1,"兵士に預ける", 1,"放っておく", 1)
			IF RESULT == 3
				PRINTFORMW 面倒事はごめんだ
				PRINTFORMW そう思った%ANAME(MASTER)%はさっさと宮殿へ戻った
				IF !(対象 <= 0)
					PRINTFORMW …どうやら%ANAME(対象)%に見られていたらしい、%ANAME(対象)%の評価が下がった
					CFLAG:対象:好感度 -= 200
				ENDIF
			ELSEIF RESULT == 2
				PRINTFORMW ここは兵士に任せる事にしよう
				PRINTFORMW %ANAME(MASTER)%は子供を近場の兵士に預けると宮殿へ戻った
			ELSEIF RESULT == 0
				PRINTFORMW 泣きだしそうな子供をなだめ、一緒に親を探してあげた
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF RAND:2 == 0
					PRINTFORMW 無事に親を見つけることが出来た！
					PRINTFORMW 笑顔で手を振る子供と別れ、良い気分で宮殿へ戻った
					IF !(対象 <= 0)
						PRINTFORMW …どうやら%ANAME(対象)%に見られていたらしい、%ANAME(対象)%の評価が上がった
						CFLAG:対象:好感度 += 200
					ENDIF
				ELSE
					PRINTFORMW 親を見つけることが出来なかった…
					PRINTFORMW 仕方なく子供を兵士に預けて宮殿へ戻った
				ENDIF
			ELSE
				PRINTFORMW 子供を見て%ANAME(対象)%は舌なめずりをした
				PRINTFORMW 泣きだしそうな子供をなだめ、「良い所に連れて行ってあげる」と囁く
				PRINTFORMW そして子供の手を引き近くの宿へと連れ込んだ
				PRINTFORML 
				PRINTFORMW 初めての事に戸惑う無垢な子供に興奮し、%ANAME(対象)%は全身で可愛がった
				CALL FORCE_FUCK(MASTER, -1, @"子供の\@RAND:2 ? ペニス # 唇\@", @"子供", RAND:2 + 1)
				PRINTFORML 
				PRINTFORMW たっぷりと楽しんだ後、子供を親元へ送り届けた

			ENDIF
	;薬を買う
	CASE 8
		PRINTFORMW 探索中、旅の薬売りと遭遇した
		PRINTFORMW 掘り出し物の薬があるらしい、値段は金5000だ
		PRINTFORMW どうしよう？
		CALL ASK_MULTI_JUDGE(@"折角だから買ってみる", MONEY >= 5000,"やめておく", 1)
			IF RESULT == 1
				PRINTFORMW あいにく今は病気でもない
				PRINTFORMW %ANAME(MASTER)%はしつこい薬売りを振り払い宮殿へ戻った
			ELSE
				PRINTFORMW 薬を買った%ANAME(MASTER)%は早速その場で飲んでみた
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW …しかし何も起きなかった
						PRINTFORMW 「効能には個人差がありますからね～、おっと返品は受け付けませんよ」
						PRINTFORMW そう告げると薬売りはさっさと去って行ってしまった
					CASE 1
						PRINTFORMW すぐに身体が熱くなった
						IF RAND:2 == 0
							PRINTFORMW 力が漲り活性化するのを感じた
							TALENT:MASTER:回復早い == 1
							TALENT:MASTER:回復遅い == 0
							PRINTFORMW %ANAME(MASTER)%は[回復早い]になった
							PRINTFORMW 「おやおや、どうやら当たりだったようで、いえいえなんでもありませんよ」
							PRINTFORMW そう告げると薬売りはさっさと去って行ってしまった
						ELSE
							PRINTFORMW 力が抜けて気怠くなるのを感じた
							TALENT:MASTER:回復遅い == 1
							TALENT:MASTER:回復早い == 0
							PRINTFORMW %ANAME(MASTER)%は[回復遅い]になった
							PRINTFORMW 「効能には個人差がありますからね～、おっと返品は受け付けませんよ」
							PRINTFORMW そう告げると薬売りはさっさと去って行ってしまった
						ENDIF
					CASE 2
						PRINTFORMW すぐに身体が火照りだした
						IF IS_MALE(MASTER)
							PRINTFORMW %ANAME(MASTER)%は思わず勃起するのを必死で押さえ込みながら薬売りを睨む
							PRINTFORMW 「効能には個人差がありますからね～、おっと返品は受け付けませんよ」
							PRINTFORMW そう告げると薬売りはさっさと去って行ってしまった
							PRINTFORMW 興奮を持て余したまま%ANAME(MASTER)%はフラフラとした足取りで宮殿へ戻った
						ELSE
							PRINTFORMW 突然子宮が疼きだし、%ANAME(MASTER)%は思わずその場にへたり込んでしまった
							PRINTFORMW 「おやおや、ひっひ…どうやら薬の副作用ですね、こちらへどうぞ、処置しましょう」
							PRINTFORMW 薬売りに腰に手を回され支えられながら、%ANAME(MASTER)%は近くの物陰に連れ込まれた
							PRINTFORML 
							PRINTFORMW %ANAME(MASTER)%はぐちょぐちょになった雌穴を薬売りによって散々調教され、何度も絶頂させられてしまった
							CALL FORCE_FUCK(MASTER, -1, @"薬売りの\@RAND:2 ? ペニス # 唇\@", @"薬売り", RAND:2 + 1)
							PRINTFORML 
							PRINTFORMW アヘ顔を晒して痙攣する%ANAME(MASTER)%を放置して薬売りはさっさと立ち去った
						ENDIF
				ENDSELECT
				MONEY -= 5000
			ENDIF
	;悪い地主を懲らしめる
	CASE 9
		金額 = MIN((MONEY / 10) + 3000 * (DAY / 10), 10000)
		SIF 金額 > MONEY
			金額 = MONEY / 2
		PRINTFORMW 領民によれば近頃この辺りの地主が横暴で困っているらしい
		PRINTFORMW 何とかしてくれと頼まれた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"地主を懲らしめる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 面倒な相手だ、領民には悪いが自分にはどうしようもない
				PRINTFORMW そう思った%ANAME(MASTER)%は宮殿へ戻った
			ELSE
				PRINTFORMW そんな不埒な輩は放置できない！
				PRINTFORMW %ANAME(MASTER)%は地主を懲らしめるための策を練った
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:MASTER:政治) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(MASTER)%は見事地主を懲らしめることに成功した！
					PRINTFORMW 領民に感謝されて宮殿へ戻った
					CALL PRINT_ADD_EXP(MASTER, "政治経験値", RAND:3 + 1, 1)
				ELSE
					IF IS_MALE(MASTER)
						PRINTFORMW やられた！
