;-------------------------------------------------
;領地探索
;-------------------------------------------------
@TERRITORYSEARCH
#DIM 対象
#DIM 仲間
#DIM 金額
#DIM 兵数
#DIM 対象都市
#DIM 候補キャラ, 1000
#DIM 候補数
VARSET 候補キャラ
VARSET 候補数

DRAWLINE
PRINTFORMW 手の空いている者に領地を探索させることにした
PRINTFORMW 誰に行かせようか？
CALL TERRITORYSEARCH_SELECT_COMMANDER
IF RESULT < 0
	PRINTFORMW やはりやめておこう
	RETURN
ENDIF
DONE_TERRITORYSEARCH = 1
対象 = RESULT
CALL SET_COOLTIME(対象, 1)
PRINTFORMW %ANAME(対象)%は領地探索に出かけた
PRINTFORML ・
PRINTFORML ・
PRINTFORMW ・
IF RAND:2 == 0
	PRINTFORMW 特にめぼしいものは見つからなかった
	RETURN
ENDIF
SELECTCASE RAND:60
	;妖怪を討伐する
	CASE 0
		PRINTFORMW 領民によれば近頃この辺りで妖怪が暴れて困っているらしい
		PRINTFORMW 何とかしてくれと頼まれた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"討伐を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 危険な相手だ、領民には悪いが自分にはどうしようもない
				PRINTFORMW そう思った%ANAME(対象)%は宮殿へ戻った
			ELSE
				PRINTFORMW そんな危険な奴は放置できない！
				PRINTFORMW %ANAME(対象)%は武器を持って妖怪の巣へと乗り込んだ
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:対象:武闘) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(対象)%は見事妖怪を討伐することに成功した
					PRINTFORMW 領民に感謝されて宮殿へ戻った
					CALL PRINT_ADD_EXP(対象, "武闘経験値", RAND:3 + 1, 1)
				ELSE
					IF IS_MALE(対象)
						PRINTFORMW やられた！
						PRINTFORMW 妖怪は想像以上に手強く、返り討ちにあった%ANAME(対象)%は命からがら逃げかえった
						CALL ADD_COOLTIME(対象, 3)
						CALL COLORPRINTFORM(@"%ANAME(対象)%は3ターンのクールダウンに入った", COLOR("注意"))
						PRINTFORMW 
					ELSE
						PRINTFORMW やられた！
						PRINTFORMW 妖怪は想像以上に手強く、%ANAME(対象)%は返り討ちにされて巣へと引きずられていった
						PRINTFORML 
						PRINTFORMW %ANAME(対象)%は服を剥ぎ取られてペニスをねじ込まれている
						PRINTFORMW 妖怪の長大なペニスで子宮を小突かれる度に%ANAME(対象)%は痙攣して喘ぎ声を上げる
						PRINTFORMW 彼女を気に入った妖怪は交尾相手として躾ける様に激しく腰を打ちつける
						PRINTFORMW %ANAME(対象)%は痛みが快楽に代わってしまうほど、野生的なセックスで何度も種付けされてしまった
						CALL FORCE_FUCK(対象, -1, @"野良妖怪の\@RAND:2 ? ペニス # 唇\@", @"野良妖怪", RAND:2 + 1)
						PRINTFORML 
						PRINTFORMW %ANAME(対象)%は散々犯された後、何とか逃げ帰った
						CALL ADD_COOLTIME(対象, 3)
						CALL COLORPRINTFORM(@"%ANAME(対象)%は3ターンのクールダウンに入った", COLOR("注意"))
						PRINTFORMW 
					ENDIF
				ENDIF
			ENDIF
	;つちのこを捕獲する
	CASE 1
		PRINTFORMW 領民によればこの辺りでつちのこを見た者がいるらしい
		PRINTFORMW 賞金も出ているので捕まえればちょっとした小遣いにもなりそうだ
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"捕獲を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW ヘビの見間違いか何かだろう
				PRINTFORMW そう思った%ANAME(対象)%は宮殿へ戻った
			ELSE
				PRINTFORMW 面白そうだ
				PRINTFORMW %ANAME(対象)%も知恵を絞って罠を張り、捕獲を試みた
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:対象:知略) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(対象)%は見事つちのこを捕獲することに成功した
					PRINTFORMW 賞金を手に入れ、ホクホク顔で宮殿へ戻った
					金額 = MIN(DAY * 50, 5000) + RAND(1000, 5000)
					MONEY += 金額
					CALL COLORPRINTFORM(@"金{金額}を手に入れた", COLOR("注意"))
					PRINTFORMW 
				ELSE
					PRINTFORMW やられた！
					PRINTFORMW 餌だけを奪われてしまった
					PRINTFORMW 本当につちのこがいたのかもわからぬまま、悔しがりながら%ANAME(対象)%は宮殿へ戻った
				ENDIF
			ENDIF
	;古びた矛を拾う
	CASE 2
		PRINTFORMW 探索中、古びた矛を見つけた
		PRINTFORMW 長い間放置されていた様だが、磨けばまだ使えるかもしれない
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"持ち帰って磨く", "武器屋に売る", "捨てておく")
			IF RESULT == 2
				PRINTFORMW こんなボロボロの武器には用はない
				PRINTFORMW そう思った%ANAME(対象)%は矛を放り投げると宮殿へ戻った
			ELSEIF RESULT == 1
				PRINTFORMW こんなものでも武器だ、もしかしたら高値で売れるかもしれない
				PRINTFORMW %ANAME(対象)%は武器屋へ持って行った
				金額 = MIN(3000 + 1000 * (DAY / 10), 10000)
				MONEY += 金額
				CALL COLORPRINTFORM(@"矛は金{金額}で売れた！", COLOR("注意"))
				PRINTFORMW 
				PRINTFORMW 思わぬ臨時収入に%ANAME(対象)%はホクホク顔で宮殿へ戻った
			ELSE
				PRINTFORMW 折角拾った武器だ、持ち帰ってみよう
				PRINTFORMW そう思った%ANAME(対象)%は矛を抱えて宮殿へ戻った
				PRINTFORMW 宮殿に戻ると早速矛を磨いてみた
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF RAND:3 == 0
					PRINTFORMW すると矛は立派な輝きを取り戻した！
					PRINTFORMW 手に持ってみると力がみなぎるのを感じた
					CALL PRINT_ADD_EXP(対象, "武闘経験値", RAND:3 + 1, 1)
				ELSE
					PRINTFORMW そこら辺にあるような普通の矛だった
					PRINTFORMW 自分には不要だと思い、部下に与えることにした
				ENDIF
			ENDIF
	;山賊を討伐する
	CASE 3
		PRINTFORMW 領民によれば近頃この辺りで山賊が出没して困っているらしい
		PRINTFORMW 何とかしてくれと頼まれた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"討伐を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 危険な相手だ、領民には悪いが自分にはどうしようもない
				PRINTFORMW そう思った%ANAME(対象)%は宮殿へ戻った
			ELSE
				PRINTFORMW そんな危険な連中は放置できない！
				PRINTFORMW %ANAME(対象)%は山賊を捕まえる為の罠を張った
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:対象:知略) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW やった！
					PRINTFORMW %ANAME(対象)%は見事山賊を討伐することに成功した！
					PRINTFORMW 領民に感謝されて宮殿へ戻った
					CALL PRINT_ADD_EXP(対象, "知略経験値", RAND:3 + 1, 1)
				ELSE
					IF IS_MALE(対象)
						PRINTFORMW やられた！
						PRINTFORMW 山賊は想像以上に手強く、返り討ちにあった%ANAME(対象)%は命からがら逃げかえった
						CALL ADD_COOLTIME(対象, 3)
						CALL COLORPRINTFORM(@"%ANAME(対象)%は3ターンのクールダウンに入った", COLOR("注意"))
						PRINTFORMW 
					ELSE
						PRINTFORMW やられた！
						PRINTFORMW 山賊は想像以上に手強く、%ANAME(対象)%は返り討ちにされてねぐらへと引きずられていった
						PRINTFORML 
						PRINTFORMW %ANAME(対象)%は山賊どもの慰み者にされている
						PRINTFORMW 抵抗する度に殴られ、お仕置きと称して犯され続け、彼女はすっかり彼らに従順に奉仕する様になっている
						PRINTFORMW 今もまた一人の山賊の上に跨り淫らに体をくねらせながら自ら腰を振っている
						PRINTFORMW 男が限界を迎えると%ANAME(対象)%はやや虚ろに笑いながら膣を締めつけ、その精を一滴残らず膣内で受け止めた
						PRINTFORMW 彼らに気に入られた%ANAME(対象)%はその後も身体中を使われてたっぷりと種付けされ続けた
						CALL FORCE_FUCK(対象, -1, @"山賊の\@RAND:2 ? ペニス # 唇\@", @"山賊", RAND:2 + 1)
						PRINTFORML 
						PRINTFORMW %ANAME(対象)%は散々犯された後、何とか逃げ帰った
						CALL ADD_COOLTIME(対象, 3)
						CALL COLORPRINTFORM(@"%ANAME(対象)%は3ターンのクールダウンに入った", COLOR("注意"))
						PRINTFORMW 
					ENDIF
				ENDIF
			ENDIF
	;お祭りを楽しむ
	CASE 4
		PRINTFORMW 領内で盛大にお祭りが開かれていた
		PRINTFORMW 久しぶりの息抜きに、%ANAME(対象)%も大いに楽しんだ
		金額 = MIN(5000 * (DAY / 10), 10000)
		SIF 金額 == 0
			金額 = 1000
		SIF 金額 > MONEY
			金額 = MONEY / 2
		MONEY -= 金額
		CALL COLORPRINTFORM(@"金{金額}を消費した", COLOR("注意"))
		PRINTFORMW 
	;茸を見つける
	CASE 5
		PRINTFORMW 森の中で不思議な形をした茸を見つけた
		PRINTFORMW 以前図鑑で見たことがある、これは珍味の茸だ！
		PRINTFORMW 毒があるらしいがきちんと調理すればとても美味らしい
		PRINTFORMW どうしよう？
		CALL ASK_MULTI(@"調理を試みる", "やめておく")
			IF RESULT == 1
				PRINTFORMW 素人が下手に手を出すべきじゃない
				PRINTFORMW そう思った%ANAME(対象)%は宮殿へ戻った
			ELSE
				PRINTFORMW 危険かもしれないが食欲には勝てなかった
				PRINTFORMW %ANAME(対象)%は茸を持ち帰り調理した
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF (ABL:対象:料理) * (RAND:5 + 1) > 30 * (RAND:10 + 1)
					PRINTFORMW 美味しい！
					PRINTFORMW 茸はまるで上等な霜降りの様に舌の上で蕩け、%ANAME(対象)%は極上の味に思わず頬が緩んだ
					PRINTFORMW %ANAME(対象)%は料理に自信を持った
					CALL PRINT_ADD_EXP(対象, "料理経験値", RAND:3 + 1, 1)
				ELSE
					PRINTFORMW しまった！
					PRINTFORMW 調理に失敗した%ANAME(対象)%は腹をこわしてしまった
					PRINTFORMW 教訓にしよう…お腹を押さえて冷や汗をかきながら%ANAME(対象)%はそう強く思った
					CALL ADD_COOLTIME(対象, 3)
					CALL COLORPRINTFORM(@"%ANAME(対象)%は3ターンのクールダウンに入った", COLOR("注意"))
					PRINTFORMW 
				ENDIF
			ENDIF
	;水浴びを愉しむ
	CASE 6
		PRINTFORMW 探索中、森の奥で水浴びに丁度良い水場を見つけた
		PRINTFORMW どうしよう？
		CALL ASK_MULTI_JUDGE(@"水浴びをする", 1,"水浴びを覗く", PENIS(対象),"水を飲む", 1)
			IF RESULT == 2
				PRINTFORMW 水はとても冷たく、飲むと力が湧いてきた
				PRINTFORMW 喉を潤した%ANAME(対象)%は充足して宮殿へ戻った
			ELSEIF RESULT == 1
				PRINTFORMW 誰かが水浴びをしている様だ
				PRINTFORMW %ANAME(対象)%はニヤリと笑い見つからない様に茂みに隠れながら覗きを敢行した
				PRINTFORML 
				PRINTFORMW 水浴びをしていたのは人間の娘だった
				PRINTFORMW まだ若さが残るものの凹凸のはっきりとした女らしい肉体がこの距離でもわかる
				PRINTFORMW 覗いているとムラムラしてきた
				PRINTFORMW どうしよう？
				CALL ASK_MULTI(@"襲いかかる", "覗き続ける")
					IF RESULT == 1
						PRINTFORMW %ANAME(対象)%は見つからない様にしながらもその魅力的な肉体を目に焼き付けた
					ELSE
						PRINTFORMW %ANAME(対象)%は周囲に他に人の気配がない事を確認すると
						PRINTFORMW 茂みを飛び出し、水浴びをしている少女に襲いかかった！
						PRINTFORMW 少女は突然の暴漢に驚き逃げようとしたが、足を滑らせ捕まってしまった
						PRINTFORMW %ANAME(対象)%は暴れる彼女を無理矢理茂みの中へと連れ込んだ
						PRINTFORML 
						PRINTFORMW 娘の膣は非常によく締まり、%ANAME(対象)%は我慢できず何度も精を放った
						PRINTFORML 
						PRINTFORMW 茫然と横たわっている娘を放置して%ANAME(対象)%は宮殿へ戻った
					ENDIF
			ELSE
				PRINTFORMW 折角だから水浴びをしていくことにした
				PRINTFORML 
				IF IS_MALE(対象)
					PRINTFORMW とても気持ちがよかった
					PRINTFORMW さっぱりした%ANAME(対象)%は充足して宮殿へ戻った
				ELSE
					PRINTFORMW %ANAME(対象)%が水浴びをしているとどこからか視線を感じた
					PRINTFORMW どうしよう？
					CALL ASK_MULTI(@"水浴びを続ける", "水から上がる")
						IF RESULT == 1
							PRINTFORMW 不穏な空気を察した%ANAME(対象)%は
							PRINTFORMW 早々に水浴びを切り上げると、足早に宮殿へ戻った
						ELSE
							PRINTFORMW 気のせいだと水浴びを続行した
							PRINTFORMW すると突然近くの茂みから痴漢が飛び出し、襲いかかってきた！
							PRINTFORMW 抵抗する%ANAME(対象)%だったが、裸の状態では何もできず、無理矢理茂みの中へと連れ込まれた
							PRINTFORML 
							PRINTFORMW %ANAME(対象)%は興奮した痴漢によって四つん這いに押し倒された
							PRINTFORMW 抵抗しようとするもそれも空しく彼のそそり立つ肉棒で貫かれてしまい、ビクンと身体を跳ねさせる
							PRINTFORMW 興奮しきった痴漢は始めから猛烈に腰を振りだし、%ANAME(対象)%は思わず喘ぎ声を出してしまう
							PRINTFORMW やがて彼が低く唸りながら熱い濁流を放つと、%ANAME(対象)%は意に反して絶頂させられてしまった
							CALL FORCE_FUCK(対象, -1, @"痴漢の\@RAND:2 ? ペニス # 唇\@", @"痴漢", RAND:2 + 1)
							PRINTFORML 
							PRINTFORMW 痴漢は秘所から白濁液を垂らしながら横たわっている%ANAME(対象)%を放置して去って行った
						ENDIF
				ENDIF
			ENDIF
	;迷子と遭遇する
	CASE 7
		FOR LOCAL, 0, CHARANUM
			IF CFLAG:(LOCAL):所属 == CFLAG:MASTER:所属 && !CFLAG:(LOCAL):9 && !IS_ANIMAL(LOCAL)
				候補キャラ:LOCAL = RAND:1000 
				候補数 ++
			ENDIF
		NEXT
		IF 候補数 == 0
			仲間 = 0
		ELSE
			仲間 = FINDELEMENT(候補キャラ, MAXARRAY(候補キャラ))
		ENDIF
		PRINTFORMW 領内を探索中、迷子の男の子と遭遇した
		PRINTFORMW 親とはぐれてしまったらしいが、周囲にそれらしい人物は見当たらない
		PRINTFORMW どうしようか？
		CALL ASK_MULTI_JUDGE(@"一緒に親を探す", 1,"誘惑する", IS_FEMALE(対象) && TALENT:対象:処女 != 1,"兵士に預ける", 1,"放っておく", 1)
			IF RESULT == 3
				PRINTFORMW 面倒事はごめんだ
				PRINTFORMW そう思った%ANAME(対象)%はさっさと宮殿へ戻った
				IF 仲間 != 0 && 対象 == MASTER
					PRINTFORMW …どうやら%ANAME(仲間)%に見られていたらしい
					CFLAG:仲間:好感度 -= 200
					CALL COLORPRINTFORM(@"%ANAME(仲間)%の評価が下がった", COLOR("注意"))
					PRINTFORMW
				ELSE
					PRINTFORMW …どうやら領民に見られていたらしい
					FOR LOCAL, 1, MAX_COUNTRY
						CALL CHANGE_RELATION_C_TO_C(LOCAL, CFLAG:MASTER:1, -10, 10)
					NEXT
					CALL COLORPRINTFORM(@"噂が広がり他国の評判が下がった", COLOR("注意"))
					PRINTFORMW 
				ENDIF
			ELSEIF RESULT == 2
				PRINTFORMW ここは兵士に任せる事にしよう
				PRINTFORMW %ANAME(対象)%は子供を近場の兵士に預けると宮殿へ戻った
			ELSEIF RESULT == 0
				PRINTFORMW 泣きだしそうな子供をなだめ、一緒に親を探してあげた
				PRINTFORML ・
				PRINTFORML ・
				PRINTFORMW ・
				IF RAND:2 == 0
					PRINTFORMW 無事に親を見つけることが出来た！
					PRINTFORMW 笑顔で手を振る子供と別れ、良い気分で宮殿へ戻った
					IF 仲間 != 0 && 対象 == MASTER
						PRINTFORMW …どうやら%ANAME(仲間)%に見られていたらしい
						CFLAG:仲間:好感度 += 200
						CALL COLORPRINTFORM(@"%ANAME(仲間)%の評価が上がった", COLOR("注意"))
						PRINTFORMW 
					ENDIF
				ELSE
					PRINTFORMW 親を見つけることが出来なかった…
					PRINTFORMW 仕方なく子供を兵士に預けて宮殿へ戻った
				ENDIF
			ELSE
				PRINTFORMW 子供を見て%ANAME(対象)%は舌なめずりをした
				PRINTFORMW 泣きだしそうな子供をなだめ、「良い所に連れて行ってあげる」と囁く
				PRINTFORMW そして子供の手を引き近くの宿へと連れ込んだ
				PRINTFORML 
				PRINTFORMW 初めての事に戸惑う無垢な子供に%ANAME(対象)%は自らの身体をさらけ出し誘惑する
				PRINTFORMW ゴクリと生唾を飲む彼に見せつけるように体をくねらせると、彼はたまらず彼女に抱きついてきた
				PRINTFORMW %ANAME(対象)%は興奮しながらも優しく彼をリードし、たっぷりとセックスを教え込む
				PRINTFORMW やがて気持ち良さそうな吐息と共に少年が射精すると、%ANAME(対象)%はぶるっと身震いしながら絶頂した
				CALL FORCE_FUCK(対象, -1, @"子供の\@RAND:2 ? ペニス # 唇\@", @"子供", RAND:2 + 1)
				PRINTFORML 
				PRINTFORMW 子供を存分に可愛がった後、親元へ送り届けた
				PRINTFORMW 別れる際、手を振る彼の股間が膨れているのが見えた

			ENDIF
	;薬を買う
	CASE 8
		PRINTFORMW 探索中、旅の薬売りと遭遇した
		PRINTFORMW 掘り出し物の薬があるらしい、値段は金5000だ
		PRINTFORMW どうしよう？
		CALL ASK_MULTI_JUDGE(@"折角だから買ってみる", MONEY >= 5000,"やめておく", 1)
			IF RESULT == 1
				PRINTFORMW あいにく今は病気でもない
				PRINTFORMW %ANAME(対象)%はしつこい薬売りを振り払い宮殿へ戻った
			ELSE
				PRINTFORMW 「へへへへ、毎度有りです」
				MONEY -= 5000
				CALL COLORPRINTFORM(@"金5000を支払った", COLOR("注意"))
				PRINTFORMW 
				PRINTFORMW 薬を買った%ANAME(対象)%は早速その場で飲んでみた
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW …しかし何も起きなかった
						PRINTFORMW 「効能には個人差がありますからね～、おっと返品は受け付けませんよ」
						PRINTFORMW そう告げると薬売りはさっさと去って行ってしまった
					CASE 1
						PRINTFORMW すぐに身体が熱くなった
						IF RAND:2 == 0
							PRINTFORMW 力が漲り活性化するのを感じた
							TALENT:対象:回復早い == 1
							TALENT:対象:回復遅い == 0
							CALL COLORPRINTFORM(@"%ANAME(対象)%は[回復早い]になった", COLOR("注意"))
